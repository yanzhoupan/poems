[
  {
    "volume": 252,
    "sequence": 1,
    "title": "宣州东峰亭各赋一物得古壁苔",
    "author": "刘太真",
    "content": [
      "苒苒温寒泉，绵绵古危壁。",
      "光含孤翠动，色与暮云寂。",
      "深浅松月间，幽人自登历。"
    ]
  },
  {
    "volume": 252,
    "sequence": 2,
    "title": "顾十二况左迁过韦苏州房杭州韦睦州…以继三君子之风焉",
    "author": "刘太真",
    "content": [
      "宠至乃不惊，罪及非无由。",
      "奔迸历畏途，缅邈赴偏陬。",
      "牧此凋弊甿，属当赋敛秋。",
      "夙兴谅无补，旬暇焉敢休。",
      "前日怀友生，独登城上楼。",
      "迢迢西北望，远思不可收。",
      "今日车骑来，旷然销人忧。",
      "晨迎东斋饭，晚度南溪游。",
      "以我碧流水，泊君青翰舟。",
      "莫将迁客程，不为胜境留。",
      "飞札谢三守，斯篇希见酬。"
    ]
  },
  {
    "volume": 252,
    "sequence": 3,
    "title": "贡院寄前主司萧尚书听（一作吕渭诗）",
    "author": "刘太真",
    "content": [
      "独坐贡闱里，愁心芳草生。",
      "山公昨夜事，应见此时情。"
    ]
  },
  {
    "volume": 252,
    "sequence": 4,
    "title": "东峰亭同刘太真各赋一物得垂涧藤",
    "author": "袁傪",
    "content": [
      "寒涧流不息，古藤终日垂。",
      "迎风仍未定，拂水更相宜。",
      "新花与旧叶，惟有幽人知。"
    ]
  },
  {
    "volume": 252,
    "sequence": 5,
    "title": "喜陆侍御破石埭草寇东峰亭赋诗",
    "author": "袁傪",
    "content": [
      "古寺东峰上，登临兴有馀。",
      "同观白简使，新报赤囊书。",
      "几处闲烽堠，千方庆里闾。",
      "欣欣夏木长，寂寂晚烟徐。",
      "战罢言归马，还师赋出车。",
      "因知越范蠡，湖海意何如。"
    ]
  },
  {
    "volume": 252,
    "sequence": 6,
    "title": "东峰亭各赋一物得岭上云",
    "author": "崔何",
    "content": [
      "伫立增远意，中峰见孤云。",
      "溶溶傍危石，片片宜夕曛。",
      "渐向群木尽，残飞更氤氲。"
    ]
  },
  {
    "volume": 252,
    "sequence": 7,
    "title": "喜陆侍御破石埭草寇东峰亭赋诗",
    "author": "崔何",
    "content": [
      "绝景西溪寺，连延近郭山。",
      "高深清扃外，行止翠微间。",
      "江澈烟尘静，川源草树闲。",
      "中丞健步到，柱史捷书还。",
      "一战清戎越，三吴变险艰。",
      "功名麟阁上，得咏入秦关。"
    ]
  },
  {
    "volume": 252,
    "sequence": 8,
    "title": "东峰亭各赋一物得幽径石",
    "author": "王纬",
    "content": [
      "片石东溪上，阴崖剩阻修。",
      "雨馀青石霭，岁晚绿苔幽。",
      "从来不可转，今日为人留。"
    ]
  },
  {
    "volume": 252,
    "sequence": 9,
    "title": "喜陆侍御破石埭草寇东峰亭赋诗",
    "author": "王纬",
    "content": [
      "蜂虿聚吴州，推贤奉圣忧。",
      "忠诚资上策，仁勇佐前筹。",
      "草木成鹅鹳，戈鋋复斗牛。",
      "戎车一战后，残垒五兵收。",
      "野静山戎险，江平水面流。",
      "更怜羁旅客，从此罢葵丘。"
    ]
  },
  {
    "volume": 252,
    "sequence": 10,
    "title": "东峰亭各赋一物得临轩桂",
    "author": "郭澹",
    "content": [
      "青青芳桂树，幽阴在庭轩。",
      "向日阴还合，从风叶乍翻。",
      "共看霜雪后，终不变凉暄。"
    ]
  },
  {
    "volume": 252,
    "sequence": 11,
    "title": "喜陆侍御破石埭草寇东峰亭赋诗",
    "author": "郭澹",
    "content": [
      "介胄鹰扬出，山林蚁聚空。",
      "忽闻飞简报，曾是坐筹功。",
      "迥夜昏氛灭，危亭眺望雄。",
      "茂勋推世上，馀兴寄杯中。",
      "喜色烟霞改，欢忻里巷同。",
      "幸兹尊俎末，饮至又从公。"
    ]
  },
  {
    "volume": 252,
    "sequence": 12,
    "title": "东峰亭各赋一物得林中翠",
    "author": "高傪",
    "content": [
      "杳霭无定状，霏微常满林。",
      "清风光不散，过雨色偏深。",
      "幽意赏难尽，终朝再招寻。"
    ]
  },
  {
    "volume": 252,
    "sequence": 13,
    "title": "东峰亭各赋一物得栖烟鸟",
    "author": "李岑1",
    "content": [
      "从来养毛羽，昔日曾飞迁。",
      "变转对朝阳，差池栖夕烟。",
      "遇此枝叶覆，夙举冀冲天。"
    ]
  },
  {
    "volume": 252,
    "sequence": 14,
    "title": "东峰亭各赋一物得寒溪草",
    "author": "苏",
    "content": [
      "幂历溪边草，游人不厌看。",
      "馀芳幽处老，深色望中寒。",
      "幸得陪情兴，青青赏未阑。"
    ]
  },
  {
    "volume": 252,
    "sequence": 15,
    "title": "东峰亭各赋一物得阴崖竹",
    "author": "袁邕",
    "content": [
      "终岁寒苔色，寂寥幽思深。",
      "朝歌犹夕岚，日永流清阴。",
      "龙钟负烟雪，自有凌云心。"
    ]
  },
  {
    "volume": 252,
    "sequence": 16,
    "title": "唐德明兴圣庙乐章·迎神",
    "author": "李纾",
    "content": [
      "元尊九德，佐尧光宅。",
      "烈祖太宗，方周作伯。",
      "响怀霜露，乐变金石。",
      "白云清风，仿佛来格。"
    ]
  },
  {
    "volume": 252,
    "sequence": 17,
    "title": "唐德明兴圣庙乐章·登歌奠币",
    "author": "李纾",
    "content": [
      "四时有典，百事来祭。",
      "尊祖奉宗，严禋大帝。",
      "礼先苍璧，奠备黝制。",
      "于万期年，熙成帝系。"
    ]
  },
  {
    "volume": 252,
    "sequence": 18,
    "title": "唐德明兴圣庙乐章·迎俎",
    "author": "李纾",
    "content": [
      "盛牲实俎，涓选休成。",
      "鼎煁阳燧，玉盥阴精。",
      "有飶嘉豆，既和大羹。",
      "侑以清乐，细齐人情。"
    ]
  },
  {
    "volume": 252,
    "sequence": 19,
    "title": "唐德明兴圣庙乐章·德明酌献",
    "author": "李纾",
    "content": [
      "清庙奕奕，和乐雍雍。",
      "器尊牺象，礼属宗公。",
      "白水方祼，黄流在中。",
      "谟明之德，万古清风。"
    ]
  },
  {
    "volume": 252,
    "sequence": 20,
    "title": "唐德明兴圣庙乐章·兴圣酌献",
    "author": "李纾",
    "content": [
      "閟宫静谧，合乐周张。",
      "泰尊始献，百末重觞。",
      "震澹存诚，庶几迪尝。",
      "遥源之祚，天汉灵长。"
    ]
  },
  {
    "volume": 252,
    "sequence": 21,
    "title": "唐德明兴圣庙乐章·亚献终献",
    "author": "李纾",
    "content": [
      "惟清惟肃，靡闻靡见。",
      "举备九成，俯终三献。",
      "庆彰曼寿，胙撤嘉荐。",
      "瘗玉埋牲，礼神斯遍。"
    ]
  },
  {
    "volume": 252,
    "sequence": 22,
    "title": "唐德明兴圣庙乐章·送神",
    "author": "李纾",
    "content": [
      "元精回复，灵贶繁滋。",
      "风洒兰路，云摇桂旗。",
      "高丘缅邈，凉部逶迟。",
      "瞻望靡及，缠绵永思。"
    ]
  },
  {
    "volume": 252,
    "sequence": 23,
    "title": "让皇帝庙乐章·迎神",
    "author": "李纾",
    "content": [
      "皇矣天宗，德先王季。",
      "因心则友，克让以位。",
      "爰命有司，式尊前志。",
      "神其降灵，昭飨祀事。"
    ]
  },
  {
    "volume": 252,
    "sequence": 24,
    "title": "让皇帝庙乐章·奠币",
    "author": "李纾",
    "content": [
      "惟帝时若，去而上仙。",
      "祀用商武，乐备宫悬。",
      "白璧加荐，玄纁告虔。",
      "子孙拜后，承兹吉蠲。"
    ]
  },
  {
    "volume": 252,
    "sequence": 25,
    "title": "让皇帝庙乐章·迎俎",
    "author": "李纾",
    "content": [
      "祀盛体荐，礼协粢盛。",
      "方周假庙，用鲁纯牲。",
      "捧撤祗敬，击拊和鸣。",
      "受釐归胙，既戒而平。"
    ]
  },
  {
    "volume": 252,
    "sequence": 26,
    "title": "让皇帝庙乐章·酌献",
    "author": "李纾",
    "content": [
      "八音具举，三寿既盥。",
      "洁兹宗彝，瑟彼圭瓒。",
      "兰肴重错，椒醑飘散。",
      "降祚维城，永为藩翰。"
    ]
  },
  {
    "volume": 252,
    "sequence": 27,
    "title": "让皇帝庙乐章·亚献终献",
    "author": "李纾",
    "content": [
      "秩礼有序，和音既同。",
      "九仪不忒，三揖将终。",
      "孝感藩后，相维辟公。",
      "四时之典，永永无穷。"
    ]
  },
  {
    "volume": 252,
    "sequence": 28,
    "title": "让皇帝庙乐章·送神",
    "author": "李纾",
    "content": [
      "奠献已事，昏昕载分。",
      "风摇雨散，灵卫絪缊。",
      "龙驾帝服，已腾五云。",
      "泮宫复閟，寂寞无闻。"
    ]
  },
  {
    "volume": 252,
    "sequence": 29,
    "title": "释奠武成王乐章·迎神",
    "author": "于邵",
    "content": [
      "卜畋不从，兆发非熊。",
      "乃倾荒政，爰佐一戎。",
      "盛烈载垂，命祀维崇。",
      "日练上戊，宿严閟宫。",
      "迎奏嘉至，感而遂通。"
    ]
  },
  {
    "volume": 252,
    "sequence": 30,
    "title": "释奠武成王乐章·奠币登歌",
    "author": "于邵",
    "content": [
      "管磬升，坛芗集。",
      "上公进，嘉币执。",
      "信以通，僾如及。",
      "恢帝功，锡后邑。",
      "四维张，百度立。",
      "绵亿载，邈难挹。"
    ]
  },
  {
    "volume": 252,
    "sequence": 31,
    "title": "释奠武成王乐章·迎俎酌献",
    "author": "于邵",
    "content": [
      "五齐絜，九牢硕。",
      "梡橛循，罍斝涤。",
      "进具物，扬鸿勣。",
      "和奏发，高灵寂。",
      "虔告终，繁祉锡。",
      "昭秩祀，永无易。"
    ]
  },
  {
    "volume": 252,
    "sequence": 32,
    "title": "释奠武成王乐章·亚献终献",
    "author": "于邵",
    "content": [
      "贰觞以献，三变其终。",
      "顾此非馨，尚达斯衷。",
      "茅缩可致，神歆载融。",
      "始神翊周，拯溺除凶。",
      "时维降祐，永绝兴戎。"
    ]
  },
  {
    "volume": 252,
    "sequence": 33,
    "title": "释奠武成王乐章·送神",
    "author": "于邵",
    "content": [
      "明祀方终，备乐斯阕。",
      "黝纁就瘗，豆笾告撤。",
      "肸蚃尚馀，光景云灭。",
      "返归虚极，神心则悦。"
    ]
  }
]