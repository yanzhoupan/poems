[
  {
    "volume": 11,
    "sequence": 1,
    "title": "郊庙歌辞·五郊乐章·黄帝宫音",
    "author": "佚名",
    "content": [
      "黄中正位，含章居贞。",
      "既长六律，兼和五声。",
      "毕陈万舞，乃荐斯牲。",
      "神其下降，永祚休平。"
    ]
  },
  {
    "volume": 11,
    "sequence": 2,
    "title": "郊庙歌辞·五郊乐章·肃和",
    "author": "佚名",
    "content": [
      "眇眇方舆，苍苍圜盖。",
      "至哉枢纽，宅中图大。",
      "气调四序，风和万籁。",
      "祚我明德，时雍道泰。"
    ]
  },
  {
    "volume": 11,
    "sequence": 3,
    "title": "郊庙歌辞·五郊乐章·雍和",
    "author": "佚名",
    "content": [
      "金县夕肆，玉俎朝陈。",
      "飨荐黄道，芬流紫宸。",
      "乃诚乃敬，载享载禋。",
      "崇荐斯在，惟皇是宾。"
    ]
  },
  {
    "volume": 11,
    "sequence": 4,
    "title": "郊庙歌辞·五郊乐章·舒和",
    "author": "佚名",
    "content": [
      "御征乘宫出郊甸，安歌率舞递将迎。",
      "自有云门符帝赏，犹持雷鼓答天成。"
    ]
  },
  {
    "volume": 11,
    "sequence": 5,
    "title": "郊庙歌辞·五郊乐章·青帝角音",
    "author": "佚名",
    "content": [
      "鹤云旦起，鸟星昏集。",
      "律候新风，阳开初蛰。",
      "至德可飨，行潦斯挹。",
      "锡以无疆，蒸人乃粒。"
    ]
  },
  {
    "volume": 11,
    "sequence": 6,
    "title": "郊庙歌辞·五郊乐章·肃和",
    "author": "佚名",
    "content": [
      "玄鸟司春，苍龙登岁。",
      "节物变柳，光风转蕙。",
      "瑶席降神，朱弦飨帝。",
      "诚备祝嘏，礼殚珪币。"
    ]
  },
  {
    "volume": 11,
    "sequence": 7,
    "title": "郊庙歌辞·五郊乐章·雍和",
    "author": "佚名",
    "content": [
      "大乐稀音，至诚简礼。",
      "文物棣棣，声明济济。",
      "六变有成，三登无体。",
      "乃眷丰洁，恩覃恺悌。"
    ]
  },
  {
    "volume": 11,
    "sequence": 8,
    "title": "郊庙歌辞·五郊乐章·舒和",
    "author": "佚名",
    "content": [
      "笙歌籥舞属年韶，鹭鼓凫钟展时豫。",
      "调露初迎绮春节，承云遽践苍霄驭。"
    ]
  },
  {
    "volume": 11,
    "sequence": 9,
    "title": "郊庙歌辞·五郊乐章·赤帝徵音",
    "author": "佚名",
    "content": [
      "青阳告谢，朱明戒序。",
      "延长是祈，敬陈椒醑。",
      "博硕斯荐，笙镛备举。",
      "庶尽肃恭，非馨稷黍。"
    ]
  },
  {
    "volume": 11,
    "sequence": 10,
    "title": "郊庙歌辞·五郊乐章·肃和",
    "author": "佚名",
    "content": [
      "离位克明，火中宵见。",
      "峰云暮起，景风晨扇。",
      "木槿初荣，含桃可荐。",
      "芬馥百品，铿锵三变。"
    ]
  },
  {
    "volume": 11,
    "sequence": 11,
    "title": "郊庙歌辞·五郊乐章·雍和",
    "author": "佚名",
    "content": [
      "昭昭丹陆，奕奕炎方。",
      "礼陈牲币，乐备篪簧。",
      "琼羞溢俎，玉醑浮觞。",
      "恭惟正直，歆此馨香。"
    ]
  },
  {
    "volume": 11,
    "sequence": 12,
    "title": "郊庙歌辞·五郊乐章·舒和",
    "author": "佚名",
    "content": [
      "千里温风飘绛羽，十枝炎景剩朱干。",
      "陈觞荐俎歌三献，拊石摐金会七盘。"
    ]
  },
  {
    "volume": 11,
    "sequence": 13,
    "title": "郊庙歌辞·五郊乐章·白帝商音",
    "author": "佚名",
    "content": [
      "白藏应节，天高气清。",
      "岁功既阜，庶类收成。",
      "万方静谧，九土和平。",
      "馨香是荐，受祚聪明。"
    ]
  },
  {
    "volume": 11,
    "sequence": 14,
    "title": "郊庙歌辞·五郊乐章·肃和",
    "author": "佚名",
    "content": [
      "金行在节，素灵居正。",
      "气肃霜严，林凋草劲。",
      "豺祭隼击，潦收川镜。",
      "九谷已登，万箱流咏。"
    ]
  },
  {
    "volume": 11,
    "sequence": 15,
    "title": "郊庙歌辞·五郊乐章·雍和",
    "author": "佚名",
    "content": [
      "律应西成，乞躔南吕。",
      "珪币咸列，笙竽备举。",
      "苾苾兰羞，芬芬桂醑。",
      "式资宴贶，用调霜序。"
    ]
  },
  {
    "volume": 11,
    "sequence": 16,
    "title": "郊庙歌辞·五郊乐章·舒和",
    "author": "佚名",
    "content": [
      "璿仪气爽惊缇籥，玉吕灰飞含素商。",
      "鸣鞞奏管芳羞荐，会舞安歌葆眊扬。"
    ]
  },
  {
    "volume": 11,
    "sequence": 17,
    "title": "郊庙歌辞·五郊乐章·黑帝羽音",
    "author": "佚名",
    "content": [
      "严冬季月，星回风厉。",
      "享祀报功，方祚来岁。"
    ]
  },
  {
    "volume": 11,
    "sequence": 18,
    "title": "郊庙歌辞·五郊乐章·肃和",
    "author": "佚名",
    "content": [
      "律回玉琯，星周金度。",
      "次极阳乌，纪穷阴兔。",
      "火林霰雪，阳泉凝冱。",
      "八蜡已登，三农息务。"
    ]
  },
  {
    "volume": 11,
    "sequence": 19,
    "title": "郊庙歌辞·五郊乐章·雍和",
    "author": "佚名",
    "content": [
      "阳月斯纪，应钟在候。",
      "载洁牲牷，爰登俎豆。",
      "既高既远，无声无臭。",
      "静言格思，惟神保祐。"
    ]
  },
  {
    "volume": 11,
    "sequence": 20,
    "title": "郊庙歌辞·五郊乐章·舒和",
    "author": "佚名",
    "content": [
      "执籥持羽初终曲，朱干玉鏚始分行。",
      "七德九功咸已畅，明灵降福具穰穰。"
    ]
  },
  {
    "volume": 11,
    "sequence": 21,
    "title": "郊庙歌辞·五郊乐章·黄郊迎神",
    "author": "佚名",
    "content": [
      "朱明季序，黄郊王辰。",
      "厚以载物，甘以养人。",
      "毓金为体，禀火成身。",
      "宫音式奏，奏以迎神。"
    ]
  },
  {
    "volume": 11,
    "sequence": 22,
    "title": "郊庙歌辞·五郊乐章·送神",
    "author": "佚名",
    "content": [
      "春末冬暮，徂夏杪秋。",
      "土王四月，时季一周。",
      "黍稷已享，笾豆宜收。",
      "送神有乐，神其赐休。"
    ]
  },
  {
    "volume": 11,
    "sequence": 23,
    "title": "郊庙歌辞·五郊乐章·青郊迎神",
    "author": "佚名",
    "content": [
      "缇幕移候，青郊启蛰。",
      "淑景迟迟，和风习习。",
      "璧玉宵备，旌旄曙立。",
      "张乐以迎，帝神其入。"
    ]
  },
  {
    "volume": 11,
    "sequence": 24,
    "title": "郊庙歌辞·五郊乐章·送神",
    "author": "佚名",
    "content": [
      "文物流彩，声明动色。",
      "人竭其恭，灵昭其饬。",
      "歆荐不已，垂祯无极。",
      "送礼有章，惟神还轼。"
    ]
  },
  {
    "volume": 11,
    "sequence": 25,
    "title": "郊庙歌辞·五郊乐章·赤郊迎神",
    "author": "佚名",
    "content": [
      "青阳节谢，朱明候改。",
      "靡草凋华，含桃流彩。",
      "虚列钟磬，筵陈脯醢。",
      "乐以迎神，神其如在。"
    ]
  },
  {
    "volume": 11,
    "sequence": 26,
    "title": "郊庙歌辞·五郊乐章·送神",
    "author": "佚名",
    "content": [
      "炎精式降，苍生攸仰。",
      "羞列豆笾，酒陈牺象。",
      "昭祀有应，冥期不爽。",
      "送乐张音，惟灵之往。"
    ]
  },
  {
    "volume": 11,
    "sequence": 27,
    "title": "郊庙歌辞·五郊乐章·白郊迎神",
    "author": "佚名",
    "content": [
      "序移玉律，节应金商。",
      "天严杀气，吹警秋方。",
      "槱燎既积，稷奠并芳。",
      "乐以迎奏，庶降神光。"
    ]
  },
  {
    "volume": 11,
    "sequence": 28,
    "title": "郊庙歌辞·五郊乐章·送神",
    "author": "佚名",
    "content": [
      "祀遵五礼，时属三秋。",
      "人怀肃敬，灵降祯休。",
      "奠歆旨酒，荐享珍羞。",
      "载张送乐，神其上游。"
    ]
  },
  {
    "volume": 11,
    "sequence": 29,
    "title": "郊庙歌辞·五郊乐章·黑郊迎神",
    "author": "佚名",
    "content": [
      "玄英戒序，黑郊临候。",
      "掌礼陈彝，司筵执豆。",
      "寒氛敛色，冱泉凝漏。",
      "乐以迎神，八音斯奏。"
    ]
  },
  {
    "volume": 11,
    "sequence": 30,
    "title": "郊庙歌辞·五郊乐章·送神",
    "author": "佚名",
    "content": [
      "北郊时冽，南陆辉处。",
      "奠本虔诚，献弥恭虑。",
      "上延祉福，下承欢豫。",
      "广乐送神，神其整驭。"
    ]
  },
  {
    "volume": 11,
    "sequence": 31,
    "title": "郊庙歌辞·朝日乐章·肃和",
    "author": "佚名",
    "content": [
      "惟圣格天，惟明飨日。",
      "帝郊肆类，王宫戒吉。",
      "珪奠春舒，钟歌晓溢。",
      "礼云克备，斯文有秩。"
    ]
  },
  {
    "volume": 11,
    "sequence": 32,
    "title": "郊庙歌辞·朝日乐章·雍和",
    "author": "佚名",
    "content": [
      "晨仪式荐，明祀惟光。",
      "神物爰止，灵晖载扬。",
      "玄端肃事，紫幄兴祥。",
      "福履攸假，于昭允王。"
    ]
  },
  {
    "volume": 11,
    "sequence": 33,
    "title": "郊庙歌辞·朝日乐章·舒和",
    "author": "佚名",
    "content": [
      "崇牙树羽延调露，旋宫扣律掩承云。",
      "诞敷懿德昭神武，载集丰功表睿文。"
    ]
  },
  {
    "volume": 11,
    "sequence": 34,
    "title": "郊庙歌辞·朝日乐章·迎神",
    "author": "佚名",
    "content": [
      "太阳朝序，王宫有仪。",
      "蟠桃彩驾，细柳光驰。",
      "轩祥表合，汉历彰奇。",
      "礼和乐备，神其降斯。"
    ]
  },
  {
    "volume": 11,
    "sequence": 35,
    "title": "郊庙歌辞·朝日乐章·送神",
    "author": "佚名",
    "content": [
      "五齐兼酌，百羞具陈。",
      "乐终广奏，礼毕崇禋。",
      "明鉴万宇，照临兆人。",
      "永流洪庆，式动曦轮。"
    ]
  },
  {
    "volume": 11,
    "sequence": 36,
    "title": "郊庙歌辞·夕月乐章·肃和",
    "author": "佚名",
    "content": [
      "测妙为神，通微曰圣。",
      "坎祀贻则，郊禋展敬。",
      "璧荐登光，金歌动映。",
      "以载嘉德，以流曾庆。"
    ]
  },
  {
    "volume": 11,
    "sequence": 37,
    "title": "郊庙歌辞·夕月乐章·雍和",
    "author": "佚名",
    "content": [
      "朏晨争举，天宗礼辟。",
      "夜典凉秋，阴明湛夕。",
      "有酉齐斯旨，有牲斯硕。",
      "穆穆其晖，穰穰是积。"
    ]
  },
  {
    "volume": 11,
    "sequence": 38,
    "title": "郊庙歌辞·祀九宫贵神乐章·豫和",
    "author": "佚名",
    "content": [
      "合吹八风金奏动，分容万舞玉鞘惊。",
      "词昭茂典光前列，夕曜乘功表盛明。"
    ]
  },
  {
    "volume": 11,
    "sequence": 39,
    "title": "郊庙歌辞·祀九宫贵神乐章·太和",
    "author": "佚名",
    "content": [
      "於昭上穹，临下有光。",
      "羽翼五佐，周流八荒。",
      "谁其飨之，时文对扬。",
      "虞经夏典，兹礼未遑。",
      "黑帝旋驭，青躔导日。",
      "金箓上玄，玉堂初吉。",
      "钩陈夕次，銮和先跸。",
      "蔼蔼群灵，昭昭咸秩。",
      "帝临中坛，受禧元神。",
      "皇灵萃止，羽旄肃陈。",
      "摄提运衡，招摇移轮。",
      "光光宇宙，电耀雷震。",
      "夜如何其，明星煌煌。",
      "天清容卫，露结坛场。",
      "树羽幢幢，佩玉锵锵。",
      "凝精驻目，瞻望神光。",
      "九位既肃，万灵毕会。",
      "天门启扃，日驭飞盖。",
      "焕兮棽离，傧兮暗霭。",
      "如山之福，惟圣时对。",
      "崇崇泰坛，灵具临兮。",
      "铿锽大乐，振动心兮。",
      "神之降矣，卿云郁兮。",
      "神之至止，清风肃兮。"
    ]
  },
  {
    "volume": 11,
    "sequence": 40,
    "title": "郊庙歌辞·祀九宫贵神乐章·肃和",
    "author": "佚名",
    "content": [
      "帝在灵坛，大明登光。",
      "天回云粹，穆穆皇皇。",
      "金奏九夏，圭陈八芗。",
      "旷哉动植，如熙春阳。"
    ]
  },
  {
    "volume": 11,
    "sequence": 41,
    "title": "郊庙歌辞·祀九宫贵神乐章·雍和",
    "author": "佚名",
    "content": [
      "歌工既奏，神位既秩。",
      "天符众星，运行太一。",
      "声和十管，气应中律。",
      "肃肃明廷，介兹元吉。"
    ]
  },
  {
    "volume": 11,
    "sequence": 42,
    "title": "郊庙歌辞·祀九宫贵神乐章·寿和",
    "author": "佚名",
    "content": [
      "俎豆有践，黄流在尊。",
      "九宫之祀，三代莫存。",
      "乐变六宫，坛开八门。",
      "圣皇昭对，祐我黎元。"
    ]
  },
  {
    "volume": 11,
    "sequence": 43,
    "title": "郊庙歌辞·祀九宫贵神乐章·福和",
    "author": "佚名",
    "content": [
      "时文哲后，肃事严禋。",
      "馨我明德，享于贵神。",
      "大庖载盈，旨酒斯醇。",
      "精意所属，期于利人。"
    ]
  },
  {
    "volume": 11,
    "sequence": 44,
    "title": "郊庙歌辞·祀九宫贵神乐章·舒和",
    "author": "佚名",
    "content": [
      "祀既云毕，明灵告旋。",
      "礼洽和应，神歆福延。",
      "动植咸若，阴阳不愆。",
      "锡兹纯嘏，天子万年。"
    ]
  },
  {
    "volume": 11,
    "sequence": 45,
    "title": "郊庙歌辞·祀九宫贵神乐章·凯安",
    "author": "佚名",
    "content": [
      "羽籥既阕干戚陈，八音克谐六变新。",
      "愉贵神兮般以乐，保皇祚兮万斯春。"
    ]
  },
  {
    "volume": 11,
    "sequence": 46,
    "title": "郊庙歌辞·祀九宫贵神乐章·肃和",
    "author": "佚名",
    "content": [
      "盛德陈万舞，威棱畅九垓。",
      "风云交律候，日月丽昭回。",
      "行庆休祥发，乘和春气来。",
      "百神肃临享，荡荡天门开。"
    ]
  },
  {
    "volume": 11,
    "sequence": 47,
    "title": "郊庙歌辞·祀九宫贵神乐章·豫和",
    "author": "佚名",
    "content": [
      "精意严恭，明祠丰洁。",
      "献酬既备，俎豆斯撤。",
      "日丽天仪，风和乐节。",
      "事光祀典，福覃有截。"
    ]
  },
  {
    "volume": 11,
    "sequence": 48,
    "title": "郊庙歌辞·祀风师乐章·迎神",
    "author": "包佶",
    "content": [
      "享申百礼，庆洽百灵。",
      "上排阊阖，洞入杳冥。",
      "奠玉高坛，燔柴广庭。",
      "神之降福，万国咸宁。"
    ]
  },
  {
    "volume": 11,
    "sequence": 49,
    "title": "郊庙歌辞·祀风师乐章·奠币登歌",
    "author": "包佶",
