[
  {
    "volume": 15,
    "sequence": 1,
    "title": "郊庙歌辞·让皇帝庙乐章·迎神",
    "author": "李舒",
    "content": [
      "皇矣天宗，德先王季。",
      "因心则友，克让以位。",
      "爰命有司，式遵前志。",
      "神其降灵，昭飨祀事。"
    ]
  },
  {
    "volume": 15,
    "sequence": 2,
    "title": "郊庙歌辞·让皇帝庙乐章·奠币",
    "author": "李舒",
    "content": [
      "惟帝时若，去而上仙。",
      "祀用商舞，乐备宫悬。",
      "白璧加荐，玄纁告虔。",
      "子孙拜后，承兹吉蠲。"
    ]
  },
  {
    "volume": 15,
    "sequence": 3,
    "title": "郊庙歌辞·让皇帝庙乐章·迎俎",
    "author": "李舒",
    "content": [
      "祀盛体荐，礼协粢盛。",
      "方周假庙，用鲁纯牲。",
      "捧彻祗敬，击拊和鸣。",
      "受釐归胙，既戒而平。"
    ]
  },
  {
    "volume": 15,
    "sequence": 4,
    "title": "郊庙歌辞·让皇帝庙乐章·酌献",
    "author": "李舒",
    "content": [
      "八音具举，三寿既盥。",
      "洁兹宗彝，瑟彼圭瓒。",
      "兰肴重错，椒醑飘散。",
      "降胙维城，永为藩翰。"
    ]
  },
  {
    "volume": 15,
    "sequence": 5,
    "title": "郊庙歌辞·让皇帝庙乐章·亚献终献",
    "author": "李舒",
    "content": [
      "秩礼有序，和音既同。",
      "九仪不忒，三揖将终。",
      "孝感藩后，相维辟公。",
      "四时之典，永永无穷。"
    ]
  },
  {
    "volume": 15,
    "sequence": 6,
    "title": "郊庙歌辞·让皇帝庙乐章·送神",
    "author": "李舒",
    "content": [
      "奠献已事，昏昕载分。",
      "风摇雨散，灵卫絪缊。",
      "龙驾帝服，上腾五云。",
      "泮宫复閟，寂寞无闻。"
    ]
  },
  {
    "volume": 15,
    "sequence": 7,
    "title": "郊庙歌辞·享隐太子庙乐章·诚和",
    "author": "佚名",
    "content": [
      "道閟鹤关，运缠鸠里。",
      "门集大命，俾歆嘉祀。",
      "礼亚六瑚，诚殚二簋。",
      "有诚颙若，神斯戾止。"
    ]
  },
  {
    "volume": 15,
    "sequence": 8,
    "title": "郊庙歌辞·享隐太子庙乐章·肃和",
    "author": "佚名",
    "content": [
      "岁肇春宗，乾开震长。",
      "瑶山既寂，戾园斯享。",
      "玉肃其事，物昭其象。",
      "弦诵成风，笙歌合响。"
    ]
  },
  {
    "volume": 15,
    "sequence": 9,
    "title": "郊庙歌辞·享隐太子庙乐章·雍和",
    "author": "佚名",
    "content": [
      "明典肃陈，神居邃启。",
      "春伯联事，秋官相礼。",
      "有来雍雍，登歌济济。",
      "缅维主鬯，庶歆芳醴。"
    ]
  },
  {
    "volume": 15,
    "sequence": 10,
    "title": "郊庙歌辞·享隐太子庙乐章·舒和",
    "author": "佚名",
    "content": [
      "三县已判歌钟列，六佾将开羽戚分。",
      "尚想燕飞来蔽日，终疑鹤影降凌云。"
    ]
  },
  {
    "volume": 15,
    "sequence": 11,
    "title": "郊庙歌辞·享隐太子庙乐章·凯安",
    "author": "佚名",
    "content": [
      "天步昔将开，商郊初欲践。",
      "抚戎金阵廓，贰极瑶图阐。",
      "鸡戟遂崇仪，龙楼期好善。",
      "弄兵隳震业，启圣隆嗣典。"
    ]
  },
  {
    "volume": 15,
    "sequence": 12,
    "title": "郊庙歌辞·隐太子庙乐章·迎神",
    "author": "佚名",
    "content": [
      "苍震有位，黄离蔽明。",
      "江充祸结，戾据灾成。",
      "衔冤昔痛，赠典今荣。",
      "享灵有秩，奉乐以迎。"
    ]
  },
  {
    "volume": 15,
    "sequence": 13,
    "title": "郊庙歌辞·隐太子庙乐章·送神",
    "author": "佚名",
    "content": [
      "皇情悼往，祀议增设。",
      "钟鼓铿锵，羽旄昭晰。",
      "掌礼云备，司筵告彻。",
      "乐以送神，灵其鉴阕。"
    ]
  },
  {
    "volume": 15,
    "sequence": 14,
    "title": "郊庙歌辞·享章怀太子庙乐章·迎神",
    "author": "佚名",
    "content": [
      "副君昭象，道应黄离。",
      "铜楼备德，玉裕成规。",
      "仙气霭霭，灵从师师。",
      "前驱戾止，控鹤来仪。"
    ]
  },
  {
    "volume": 15,
    "sequence": 15,
    "title": "郊庙歌辞·享章怀太子庙乐章·登歌酌鬯",
    "author": "佚名",
    "content": [
      "忠孝本著，羽翼先成。",
      "寝门昭德，驰道为程。",
      "币帛有典，容卫无声。",
      "司存既肃，庙享惟清。"
    ]
  },
  {
    "volume": 15,
    "sequence": 16,
    "title": "郊庙歌辞·享章怀太子庙乐章·迎俎酌献",
    "author": "佚名",
    "content": [
      "通三锡胤，明两承英。",
      "太山比赫，伊水闻笙。",
      "宗祧是寄，礼乐其亨。",
      "嘉辰荐俎，以发声明。"
    ]
  },
  {
    "volume": 15,
    "sequence": 17,
    "title": "郊庙歌辞·享章怀太子庙乐章·送文舞迎武舞",
    "author": "佚名",
    "content": [
      "羽籥崇文礼以毕，干戚奋武事将行。",
      "用舍繇来其有致，壮志宣威乐太平。"
    ]
  },
  {
    "volume": 15,
    "sequence": 18,
    "title": "郊庙歌辞·享章怀太子庙乐章·武舞作",
    "author": "佚名",
    "content": [
      "绿林炽炎历，黄虞格有苗。",
      "沙尘惊塞外，帷幄命嫖姚。",
      "七德干戈止，三边云雾消。",
      "宝祚长无极，歌舞盛今朝。"
    ]
  },
  {
    "volume": 15,
    "sequence": 19,
    "title": "郊庙歌辞·享懿德太子庙乐章·迎神",
    "author": "佚名",
    "content": [
      "甲观昭祥，画堂升位。",
      "礼绝群后，望尊储贰。",
      "启诵惭德，庄丕掩粹。",
      "伊浦凤翔，缑峰鹤至。"
    ]
  },
  {
    "volume": 15,
    "sequence": 20,
    "title": "郊庙歌辞·享懿德太子庙乐章·登歌酌鬯",
    "author": "佚名",
    "content": [
      "誉阐元储，寄崇明两。",
      "玉裕虽晦，铜楼可想。",
      "弦诵辍音，笙歌罢响。",
      "币帛言设，礼容无爽。"
    ]
  },
  {
    "volume": 15,
    "sequence": 21,
    "title": "郊庙歌辞·享懿德太子庙乐章·迎俎酌献",
    "author": "佚名",
    "content": [
      "雍雍盛典，肃肃灵祠。",
      "宾天有圣，对日无期。",
      "飘飖羽服，掣曳云旗。",
      "眷言主鬯，心乎怆兹。"
    ]
  },
  {
    "volume": 15,
    "sequence": 22,
    "title": "郊庙歌辞·享懿德太子庙乐章·送文舞迎武舞",
    "author": "佚名",
    "content": [
      "八音协奏陈金石，六佾分行整礼容。",
      "沧溟赴海还称少，素月开轮即是重。"
    ]
  },
  {
    "volume": 15,
    "sequence": 23,
    "title": "郊庙歌辞·享懿德太子庙乐章·武舞作",
    "author": "佚名",
    "content": [
      "隋季昔云终，唐年初启圣。",
      "纂戎将禁暴，崇儒更敷政。",
      "威略静三边，仁恩覃万姓。"
    ]
  },
  {
    "volume": 15,
    "sequence": 24,
    "title": "郊庙歌辞·享节愍太子庙乐章·迎神",
    "author": "佚名",
    "content": [
      "储后望崇，元良寄切。",
      "寝门是仰，驰道不绝。",
      "仙袂云会，灵旗电晰。",
      "煌煌而来，礼物攸设。"
    ]
  },
  {
    "volume": 15,
    "sequence": 25,
    "title": "郊庙歌辞·享节愍太子庙乐章·登歌酌鬯",
    "author": "佚名",
    "content": [
      "灼灼重明，仰承元首。",
      "既贤且哲，惟孝与友。",
      "惟孝虽遥，灵规不朽。",
      "礼因诚致，备絜玄酒。"
    ]
  },
  {
    "volume": 15,
    "sequence": 26,
    "title": "郊庙歌辞·享节愍太子庙乐章·迎俎酌献",
    "author": "佚名",
    "content": [
      "嘉荐有典，至诚莫骞。",
      "画梁云亘，雕俎星连。",
      "乐器周列，礼容备宣。",
      "依稀如在，若未宾天。"
    ]
  },
  {
    "volume": 15,
    "sequence": 27,
    "title": "郊庙歌辞·享节愍太子庙乐章·送文舞出迎武舞入",
    "author": "佚名",
    "content": [
      "邕邕阐化凭文德，赫赫宣威藉武功。",
      "既执羽旄先拂吹，还持玉戚更挥空。"
    ]
  },
  {
    "volume": 15,
    "sequence": 28,
    "title": "郊庙歌辞·享节愍太子庙乐章·武舞作",
    "author": "佚名",
    "content": [
      "武德谅雍雍，由来扫寇戎。",
      "剑光挥作电，旗影列成虹。",
      "雾廓三边静，波澄四海同。",
      "睿图今已盛，相共舞皇风。"
    ]
  },
  {
    "volume": 15,
    "sequence": 29,
    "title": "郊庙歌辞·享文敬太子庙乐章·请神",
    "author": "许孟容",
    "content": [
      "觞牢具品，管磬有节。",
      "祝道夤恭，神仪昭晰。",
      "桐圭早贵，象辂追设。",
      "声达乐成，降歆丰洁。"
    ]
  },
  {
    "volume": 15,
    "sequence": 30,
    "title": "郊庙歌辞·享文敬太子庙乐章·登歌",
    "author": "陈京",
    "content": [
      "歌以德发，声以乐贵。",
      "乐善名存，追仙礼异。",
      "鸾旌拱修，凤鸣合吹。",
      "神听皇慈，仲月皆至。"
    ]
  },
  {
    "volume": 15,
    "sequence": 31,
    "title": "郊庙歌辞·享文敬太子庙乐章·迎俎酌献",
    "author": "冯伉",
    "content": [
      "撰日瞻景，诚陈乐张。",
      "礼容秩秩，羽舞煌煌。",
      "肃将涤濯，祗荐芬芳。",
      "永锡繁祉，思深飨尝。"
    ]
  },
  {
    "volume": 15,
    "sequence": 32,
    "title": "郊庙歌辞·享文敬太子庙乐章·送文舞迎武舞",
    "author": "佚名",
    "content": [
      "干旄羽籥相亏蔽，一进一退殊行缀。",
      "昔献三雍盛礼容，今陈六佾崇仪制。"
    ]
  },
  {
    "volume": 15,
    "sequence": 33,
    "title": "郊庙歌辞·享文敬太子庙乐章·亚献终献",
    "author": "崔邠",
    "content": [
      "醴齐泛尊彝，轩县动干戚。",
      "入室僾如在，升阶虔所历。",
      "奋疾合威容，定利舒皦绎。",
      "方崇庙貌礼，永被君恩锡。"
    ]
  },
  {
    "volume": 15,
    "sequence": 34,
    "title": "郊庙歌辞·享文敬太子庙乐章·送神",
    "author": "崔邠",
    "content": [
      "三献具举，九旗将旋。",
      "追劳表德，罢享宾天。",
      "风引仙管，堂虚画筵。",
      "芳馨常在，瞻望悠然。"
    ]
  },
  {
    "volume": 15,
    "sequence": 35,
    "title": "郊庙歌辞·享惠昭太子庙乐章·请神",
    "author": "归登",
    "content": [
      "嘉荐既陈，祀事孔明。",
      "闲歌在堂，万舞在庭。",
      "外则尽物，内则尽诚。",
      "凤笙如闻，歌其洁精。"
    ]
  },
  {
    "volume": 15,
    "sequence": 36,
    "title": "郊庙歌辞·享惠昭太子庙乐章·登歌",
    "author": "杜羔",
    "content": [
      "因心克孝，位震遗芬。",
      "宾天道茂，轸怀气分。",
      "发祗乃祀，咳叹如闻。",
      "二歌斯升，以咏德薰。"
    ]
  },
  {
    "volume": 15,
    "sequence": 37,
    "title": "郊庙歌辞·享惠昭太子庙乐章·迎俎酌献",
    "author": "李逢吉",
    "content": [
      "既洁酒醴，聿陈熟腥。",
      "肃将震念，昭格储灵。",
      "展矣礼典，薰然德馨。",
      "愔愔管磬，亦具是听。"
    ]
  },
  {
    "volume": 15,
    "sequence": 38,
    "title": "郊庙歌辞·享惠昭太子庙乐章·送文舞出迎武舞入",
    "author": "孟简",
    "content": [
      "喧喧金石容既缺，肃肃羽驾就行列。",
      "缑山遗响昔所闻，庙庭进旅今攸设。"
    ]
  },
  {
    "volume": 15,
    "sequence": 39,
    "title": "郊庙歌辞·享惠昭太子庙乐章·亚献终献",
    "author": "裴度",
    "content": [
      "重轮始发祥，齿胄方兴学。",
      "冥然升紫府，铿尔荐清乐。",
      "奠斝致馨香，在庭纷羽籥。",
      "礼成神既醉，仿佛缑山鹤。"
    ]
  },
  {
    "volume": 15,
    "sequence": 40,
    "title": "郊庙歌辞·享惠昭太子庙乐章·送神",
    "author": "王涯",
    "content": [
      "威仪毕陈，备乐将阕。",
      "苞茅酒缩，膋萧香彻。",
      "宫臣展事，肃雍在列。",
      "迎精送往，厥鉴昭晰。"
    ]
  },
  {
    "volume": 15,
    "sequence": 41,
    "title": "郊庙歌辞·武后崇先庙乐章（载《本集》）",
    "author": "佚名",
    "content": [
      "先德谦撝冠昔，严规节素超今。",
      "奉国忠诚每竭，承家至孝纯深。",
      "追崇惧乖尊意，显号恐玷徽音。",
      "既迫王公屡请，方乃俯遂群心。",
      "有限无由展敬，奠醑每阙亲斟。",
      "大礼虔申典册，蘋藻敬荐翘襟。"
    ]
  },
  {
    "volume": 15,
    "sequence": 42,
    "title": "郊庙歌辞·褒德庙乐章·昭德",
    "author": "佚名",
    "content": [
      "道赫梧宫，悲盈蒿里。",
      "爰赐徽烈，载敷嘉祀。",
      "享洽四时，规陈二簋。",
      "灵应昭格，神其戾止。"
    ]
  },
  {
    "volume": 15,
    "sequence": 43,
    "title": "郊庙歌辞·褒德庙乐章·进德",
    "author": "佚名",
    "content": [
      "涂山懿戚，妫汭崇姻。",
      "祠筵肇启，祭典方申。",
      "礼以备物，乐以感神。",
      "用隆敦叙，载穆彝伦。"
    ]
  },
  {
    "volume": 15,
    "sequence": 44,
    "title": "郊庙歌辞·褒德庙乐章·褒德",
    "author": "佚名",
    "content": [
      "家著累仁，门昭积善。",
      "瑶篚既列，金县式展。"
    ]
  },
  {
    "volume": 15,
    "sequence": 45,
    "title": "郊庙歌辞·褒德庙乐章·武舞作",
    "author": "佚名",
    "content": [
      "昭昭竹殿开，奕奕兰宫启。",
      "懿范隆丹掖，殊荣辟朱邸。",
      "六佾荐徽容，三簋陈芳醴。",
      "万石覃贻厥，分珪崇祖祢。"
    ]
  },
  {
    "volume": 15,
    "sequence": 46,
    "title": "郊庙歌辞·褒德庙乐章·彰德",
    "author": "佚名",
    "content": [
      "名隆五岳，秩映三台。",
      "严祠已备，睟影方回。"
    ]
  },
  {
    "volume": 15,
    "sequence": 47,
    "title": "舞曲歌辞·功成庆善乐舞词",
    "author": "佚名",
    "content": [
      "寿丘惟旧迹，酆邑乃前基。",
      "粤余承累圣，悬弧亦在兹。",
      "弱龄逢运改，提剑郁匡时。",
      "指麾八荒定，怀柔万国夷。",
      "梯山盛入款，驾海亦来思。",
      "单于陪武帐，日逐卫文木蓖。",
      "端扆朝四岳，无为任百司。",
      "霜节明秋景，轻冰结水湄。",
      "芸黄遍原隰，禾颖积京坻。",
      "共乐还谯宴，欢此大风诗。"
    ]
  },
  {
