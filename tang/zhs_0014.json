[
  {
    "volume": 14,
    "sequence": 1,
    "title": "郊庙歌辞·太清宫乐章·煌煌",
    "author": "佚名",
    "content": [
      "煌煌道宫，肃肃太清。",
      "礼光尊祖，乐备充庭。",
      "罄竭诚至，希夷降灵。",
      "云凝翠盖，风焰红旌。",
      "众真以从，九奏初迎。",
      "永惟休祐，是锡和平。"
    ]
  },
  {
    "volume": 14,
    "sequence": 2,
    "title": "郊庙歌辞·太清宫乐章·冲和",
    "author": "佚名",
    "content": [
      "虚无结思，钟磬和音。",
      "歌以颂德，香以达心。",
      "礼殊祼鬯，义感昭临。",
      "灵车至止，庆垂愔愔。"
    ]
  },
  {
    "volume": 14,
    "sequence": 3,
    "title": "郊庙歌辞·太清宫乐章·香初上",
    "author": "佚名",
    "content": [
      "肃肃我祖，绵绵道宗。",
      "至感潜达，灵心暗通。",
      "云輧御气，芝盖随风。",
      "四时禋祀，万国来同。"
    ]
  },
  {
    "volume": 14,
    "sequence": 4,
    "title": "郊庙歌辞·太清宫乐章·再上",
    "author": "佚名",
    "content": [
      "仙宗绩道，我李承天。",
      "庆深虚极，符光象先。",
      "俗登仁寿，化阐蟺涓。",
      "五千贻范，亿万斯年。"
    ]
  },
  {
    "volume": 14,
    "sequence": 5,
    "title": "郊庙歌辞·太清宫乐章·终上",
    "author": "佚名",
    "content": [
      "不宰元功，无为上圣。",
      "洪源长发，诞受天命。",
      "金奏迎真，璇宫展盛。",
      "备礼周乐，垂光储庆。"
    ]
  },
  {
    "volume": 14,
    "sequence": 6,
    "title": "郊庙歌辞·太清宫乐章·紫极舞",
    "author": "佚名",
    "content": [
      "至道生元气，重圆法混成。",
      "无为观大象，冲用体常名。",
      "仙乐临丹阙，云车出玉京。",
      "灵符百代应，瑞节九贞迎。",
      "宝运开皇极，天临映太清。",
      "长垂一德庆，永庇万方宁。"
    ]
  },
  {
    "volume": 14,
    "sequence": 7,
    "title": "郊庙歌辞·太清宫乐章·序入破第一奏",
    "author": "佚名",
    "content": [
      "真宗开妙理，冲教统清虚。",
      "化演无为日，言昭有象初。",
      "瑶台肃灵瑞，金阙映仙居。",
      "一奏三清乐，长回八景舆。"
    ]
  },
  {
    "volume": 14,
    "sequence": 8,
    "title": "郊庙歌辞·太清宫乐章·第二奏",
    "author": "佚名",
    "content": [
      "虚极仙宗本，希夷象帝先。",
      "百灵朝太上，万法祖重圆。",
      "善贷惟冲德，功成谓自然。",
      "云门达和气，思用合钧天。"
    ]
  },
  {
    "volume": 14,
    "sequence": 9,
    "title": "郊庙歌辞·太清宫乐章·第三奏",
    "author": "佚名",
    "content": [
      "元符传紫极，宝祚启高真。",
      "道德先垂裕，冲和已化淳。",
      "人风齐太古，天瑞叶惟新。",
      "仙乐清都上，长明交泰辰。"
    ]
  },
  {
    "volume": 14,
    "sequence": 10,
    "title": "郊庙歌辞·太清宫乐章·登歌",
    "author": "佚名",
    "content": [
      "严禋展事，理洁烝尝。",
      "皇矣圣祖，德惟馨香。",
      "盛荐既撤，工歌载扬。",
      "大来之庆，降福穰穰。"
    ]
  },
  {
    "volume": 14,
    "sequence": 11,
    "title": "郊庙歌辞·太清宫乐章·真和",
    "author": "佚名",
    "content": [
      "玉磬含香，金炉既馥。",
      "风驭泠泠，云坛肃肃。",
      "杳归大象，霈流嘉福。",
      "俾宁万邦，无思不服。"
    ]
  },
  {
    "volume": 14,
    "sequence": 12,
    "title": "郊庙歌辞·德明兴圣庙乐章·迎神",
    "author": "佚名",
    "content": [
      "元尊九德，佐尧光宅。",
      "烈祖太宗，方周作伯。",
      "响怀霜露，乐变金石。",
      "白云清风，仿佛来格。"
    ]
  },
  {
    "volume": 14,
    "sequence": 13,
    "title": "郊庙歌辞·德明兴圣庙乐章·登歌奠币",
    "author": "佚名",
    "content": [
      "四时有典，百事来祭。",
      "尊祖奉宗，严禋大帝。",
      "礼先苍璧，奠币黝制。",
      "于斯万年，熙成帝系。"
    ]
  },
  {
    "volume": 14,
    "sequence": 14,
    "title": "郊庙歌辞·德明兴圣庙乐章·迎俎",
    "author": "佚名",
    "content": [
      "盛牲实俎，涓选休成。",
      "鼎煁阳燧，玉盥阴精。",
      "有飶嘉豆，既和大羹。",
      "侑以清乐，细齐人情。"
    ]
  },
  {
    "volume": 14,
    "sequence": 15,
    "title": "郊庙歌辞·德明兴圣庙乐章·德明酌献",
    "author": "佚名",
    "content": [
      "清庙奕奕，和乐雍雍。",
      "器尊牺象，礼属宗公。",
      "白水方祼，黄流在中。",
      "谟明之德，万古清风。"
    ]
  },
  {
    "volume": 14,
    "sequence": 16,
    "title": "郊庙歌辞·德明兴圣庙乐章·兴圣酌献",
    "author": "佚名",
    "content": [
      "閟宫静谧，合乐周张。",
      "泰奠始献，百末重觞。",
      "震澹存诚，庶几迪尝。",
      "遥源之祚，天汉灵长。"
    ]
  },
  {
    "volume": 14,
    "sequence": 17,
    "title": "郊庙歌辞·德明兴圣庙乐章·亚献终献",
    "author": "佚名",
    "content": [
      "惟清惟肃，靡闻靡见。",
      "举备九成，俯终三献。",
      "庆彰曼寿，胙彻嘉荐。",
      "瘗玉埋牲，礼神斯遍。"
    ]
  },
  {
    "volume": 14,
    "sequence": 18,
    "title": "郊庙歌辞·德明兴圣庙乐章·送神",
    "author": "佚名",
    "content": [
      "元精回复，灵贶繁滋。",
      "风洒兰路，云摇桂旗。",
      "高丘缅邈，凉部逶迟。",
      "瞻望靡及，缠绵永思。"
    ]
  },
  {
    "volume": 14,
    "sequence": 19,
    "title": "郊庙歌辞·仪坤庙乐章·永和",
    "author": "徐彦伯",
    "content": [
      "猗若清庙，肃肃荧荧。",
      "国荐严祀，坤兴淑灵。",
      "有几在室，有乐在庭。",
      "临兹孝享，百禄惟宁。"
    ]
  },
  {
    "volume": 14,
    "sequence": 20,
    "title": "郊庙歌辞·仪坤庙乐章·金奏（一本无此章）",
    "author": "佚名",
    "content": [
      "阴灵曜祉，轩曜降精。",
      "祥符淑气，庆集柔明。",
      "瑶俎既列，雕桐发声。",
      "徽猷永远，比德皇英。"
    ]
  },
  {
    "volume": 14,
    "sequence": 21,
    "title": "郊庙歌辞·仪坤庙乐章·太和",
    "author": "丘说",
    "content": [
      "孝哉我后，冲乎乃圣。",
      "道映重华，德辉文命。",
      "慕深视箧，情殷抚镜。",
      "万国移风，兆人承庆。"
    ]
  },
  {
    "volume": 14,
    "sequence": 22,
    "title": "郊庙歌辞·仪坤庙乐章·肃和",
    "author": "张齐贤",
    "content": [
      "祼圭既濯，郁鬯既陈。",
      "画幕云举，黄流玉醇。",
      "仪充献酌，礼盛众禋。",
      "地察惟孝，愉焉飨亲。"
    ]
  },
  {
    "volume": 14,
    "sequence": 23,
    "title": "郊庙歌辞·仪坤庙乐章·雍和",
    "author": "郑善玉",
    "content": [
      "酌郁既灌，芗萧方爇。",
      "笾豆静器，簠簋芬飶。",
      "鱼腊荐美，牲牷表洁。",
      "是戢是将，载迎载列。"
    ]
  },
  {
    "volume": 14,
    "sequence": 24,
    "title": "郊庙歌辞·仪坤庙乐章·昭升",
    "author": "薛稷",
    "content": [
      "阳灵配德，阴魄昭升。",
      "尧坛凤下，汉室龙兴。",
      "伣天作对，前旒是凝。",
      "化行南国，道盛西陵，造舟集灌，无德而称。",
      "我粢既絜，我醴既澄。",
      "阴阴灵庙，光灵若凭。",
      "德馨惟享，孝思烝烝。"
    ]
  },
  {
    "volume": 14,
    "sequence": 25,
    "title": "郊庙歌辞·仪坤庙乐章·坤贞",
    "author": "佚名",
    "content": [
      "乾道既亨，坤元以贞。",
      "肃雍攸在，辅佐斯成。",
      "外睦九族，内光一庭。",
      "克生睿哲，祚我休明。",
      "钦若徽范，悠哉淑灵。",
      "建兹清宫，于彼上京。",
      "缩茅以献，絜秬惟馨。",
      "实受其福，期乎亿龄。"
    ]
  },
  {
    "volume": 14,
    "sequence": 26,
    "title": "郊庙歌辞·仪坤庙乐章·寿和",
    "author": "徐坚",
    "content": [
      "於穆清庙，肃雍严祀。",
      "合福受釐，介以繁社。"
    ]
  },
  {
    "volume": 14,
    "sequence": 27,
    "title": "郊庙歌辞·仪坤庙乐章·舒和",
    "author": "胡雄",
    "content": [
      "送文迎武递参差，一始一终光圣仪。",
      "四海生人歌有庆，千龄孝享肃无亏。"
    ]
  },
  {
    "volume": 14,
    "sequence": 28,
    "title": "郊庙歌辞·仪坤庙乐章·安和",
    "author": "刘子玄",
    "content": [
      "妙算申帷幄，神谋及庙庭。",
      "两阶文物备，七德武功成。",
      "校猎长杨苑，屯军细柳营。",
      "将军献凯入，歌舞溢重城。"
    ]
  },
  {
    "volume": 14,
    "sequence": 29,
    "title": "郊庙歌辞·仪坤庙乐章·雍和",
    "author": "员半千",
    "content": [
      "孝享云毕，惟彻有章。",
      "云感玄羽，风凄素商。",
      "瞻望神座，祗恋匪遑。",
      "礼终乐阕，肃雍锵锵。"
    ]
  },
  {
    "volume": 14,
    "sequence": 30,
    "title": "郊庙歌辞·仪坤庙乐章·永和",
    "author": "祝钦明",
    "content": [
      "閟宫实实，清庙微微。",
      "降格无象，馨香有依。",
      "式昭纂庆，方融嗣徽。",
      "明禋是享，神保聿归。"
    ]
  },
  {
    "volume": 14,
    "sequence": 31,
    "title": "郊庙歌辞·仪坤庙乐章·迎神",
    "author": "佚名",
    "content": [
      "月灵降德，坤元授光。",
      "娥英比秀，任姒均芳。",
      "瑶台荐祉，金屋延祥。",
      "迎神有乐，歆此嘉芗。"
    ]
  },
  {
    "volume": 14,
    "sequence": 32,
    "title": "郊庙歌辞·仪坤庙乐章·送神",
    "author": "佚名",
    "content": [
      "太帛仪大，金丝奏广。",
      "灵应有孚，冥征不爽。",
      "降彼休福，歆兹禋享。",
      "送乐有章，神麾其上。"
    ]
  },
  {
    "volume": 14,
    "sequence": 33,
    "title": "郊庙歌辞·昭德皇后庙乐章·永和",
    "author": "佚名",
    "content": [
      "穆清庙，荐严禋。",
      "昭礼备，和乐新。",
      "望灵光，集元辰。",
      "祚无极，享万春。"
    ]
  },
  {
    "volume": 14,
    "sequence": 34,
    "title": "郊庙歌辞·昭德皇后庙乐章·肃和",
    "author": "佚名",
    "content": [
      "诚心达，娱乐分。",
      "升箫膋，郁氛氲。",
      "茅既缩，鬯既薰。",
      "后来思，福如云。"
    ]
  },
  {
    "volume": 14,
    "sequence": 35,
    "title": "郊庙歌辞·昭德皇后庙乐章·雍和",
    "author": "佚名",
    "content": [
      "我将我享，尽明而诚。",
      "载芬黍稷，载涤牺牲。",
      "懿矣元良，万邦以贞。",
      "心乎爱敬，若睹容声。"
    ]
  },
  {
    "volume": 14,
    "sequence": 36,
    "title": "郊庙歌辞·昭德皇后庙乐章·坤元",
    "author": "佚名",
    "content": [
      "於穆先后，丽圣称崇。",
      "母临万宇，道被六宫。",
      "昌时协庆，理内成功。",
      "殷荐明德，传芳国风。"
    ]
  },
  {
    "volume": 14,
    "sequence": 37,
    "title": "郊庙歌辞·昭德皇后庙乐章·寿和",
    "author": "佚名",
    "content": [
      "工祝致告，徽音不遐。",
      "酒醴咸旨，馨香具嘉。",
      "受釐献祉，永庆邦家。"
    ]
  },
  {
    "volume": 14,
    "sequence": 38,
    "title": "郊庙歌辞·昭德皇后庙乐章·舒和",
    "author": "佚名",
    "content": [
      "金枝羽部彻清歌，瑶台肃穆笙磬罗。",
      "谐音遍响合明意，万类昭融灵应多。"
    ]
  },
  {
    "volume": 14,
    "sequence": 39,
    "title": "郊庙歌辞·昭德皇后庙乐章·凯安",
    "author": "佚名",
    "content": [
      "辰位列四星，帝功参十乱。",
      "进贤勤内辅，扈跸清多难。",
      "承天厚载均，并耀宵光灿。",
      "留徽蔼前躅，万古披图焕。"
    ]
  },
  {
    "volume": 14,
    "sequence": 40,
    "title": "郊庙歌辞·昭德皇后庙乐章·雍和",
    "author": "佚名",
    "content": [
      "公尸既起，享礼载终。",
      "称歌进彻，尽敬繇衷。",
      "泽流惠下，大小咸同。"
    ]
  },
  {
    "volume": 14,
    "sequence": 41,
    "title": "郊庙歌辞·昭德皇后庙乐章·永和",
    "author": "佚名",
    "content": [
      "昭事终，幽享馀。",
      "移月御，返仙居。",
      "璇庭寂，灵幄虚。",
      "顾裴回，感皇储。"
    ]
  }
]