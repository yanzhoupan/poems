[
  {
    "volume": 16,
    "sequence": 1,
    "title": "郊庙歌辞·梁郊祀乐章·庆和",
    "author": "赵光逢",
    "content": [
      "就阳位，升圆丘。",
      "佩双玉，御大裘。",
      "膺天命，拥神休。",
      "万灵感，百禄遒。",
      "秉黄钺，建朱旗。",
      "震八表，清二仪。",
      "帝业显，王道夷。",
      "受景命，启皇基。",
      "开九门，怀百神。",
      "通肸蚃，接氤氲。",
      "明粢荐，广乐陈。",
      "奠嘉璧，燎芳薪。",
      "膺宝图，执左契。",
      "德应天，圣飨帝。",
      "荐表衷，荷灵惠。",
      "寿万年，祚百世。",
      "惟德动天，有感必通。",
      "秉兹一德，禋于六宗。",
      "钦夤宝命，恭肃礼容。",
      "来顾来享，永穆皇风。",
      "天惟佑德，辟乃奉天。",
      "交感斯在，昭事罔愆。",
      "岁功已就，王道无偏。",
      "于焉报本，是用告虔。"
    ]
  },
  {
    "volume": 16,
    "sequence": 2,
    "title": "郊庙歌辞·梁郊祀乐章·庆顺",
    "author": "佚名",
    "content": [
      "圣皇戾止，天步舒迟。",
      "乾乾睿相，穆穆皇仪。",
      "进退必肃，陟降是祗。",
      "六变克协，万灵协随。"
    ]
  },
  {
    "volume": 16,
    "sequence": 3,
    "title": "郊庙歌辞·梁郊祀乐章·庆平",
    "author": "佚名",
    "content": [
      "天命降鉴，帝德惟馨。",
      "享祀不忒，礼容孔明。",
      "奠璧布币，荐神献精。",
      "神祐以答，敷锡永宁。"
    ]
  },
  {
    "volume": 16,
    "sequence": 4,
    "title": "郊庙歌辞·梁郊祀乐章·庆肃",
    "author": "张衮",
    "content": [
      "笾豆簠簋，黍稷非馨。",
      "懿兹彝器，厥德惟明。",
      "金石匏革，以和以平。",
      "由此无体，期乎永宁。"
    ]
  },
  {
    "volume": 16,
    "sequence": 5,
    "title": "郊庙歌辞·梁郊祀乐章·庆熙",
    "author": "佚名",
    "content": [
      "哲后躬享，旨酒斯陈。",
      "王恭无斁，严祀维夤。",
      "皇祖以配，大孝以振。",
      "宜锡景福，永休下民。"
    ]
  },
  {
    "volume": 16,
    "sequence": 6,
    "title": "郊庙歌辞·梁郊祀乐章·庆隆",
    "author": "佚名",
    "content": [
      "恭祀上帝，于国之阳。",
      "爵醴是荷，鸿基永昌。"
    ]
  },
  {
    "volume": 16,
    "sequence": 7,
    "title": "郊庙歌辞·梁郊祀乐章·庆融",
    "author": "佚名",
    "content": [
      "道和气兮袭氤氲，宣皇规兮彰圣神。",
      "服遐裔兮敷质文，格苗扈兮息烟尘。"
    ]
  },
  {
    "volume": 16,
    "sequence": 8,
    "title": "郊庙歌辞·梁郊祀乐章·庆休",
    "author": "佚名",
    "content": [
      "大业来四夷，仁风和万国。",
      "白日体无私，皇天辅有德。",
      "七旬罪已服，六月师方克。",
      "伟哉帝道隆，终始常作则。"
    ]
  },
  {
    "volume": 16,
    "sequence": 9,
    "title": "郊庙歌辞·梁郊祀乐章·庆和",
    "author": "佚名",
    "content": [
      "烟燎升，礼容彻。",
      "诚感达，人神悦。",
      "灵贶彰，圣情结。",
      "玉座寂，金炉歇。"
    ]
  },
  {
    "volume": 16,
    "sequence": 10,
    "title": "郊庙歌辞·周郊祀乐章·昭顺乐",
    "author": "佚名",
    "content": [
      "五兵勿用，万国咸安。",
      "告功圆盖，受命云坛。",
      "乐鸣凤律，礼备鸡竿。",
      "神光欲降，众目遐观。"
    ]
  },
  {
    "volume": 16,
    "sequence": 11,
    "title": "郊庙歌辞·周郊祀乐章·治顺乐",
    "author": "佚名",
    "content": [
      "羽卫离丹阙，金轩赴泰坛。",
      "珠旗明月色，玉佩晓霜寒。",
      "黼黻龙衣备，琮璜宝器完。",
      "百神将受职，宗社保长安。"
    ]
  },
  {
    "volume": 16,
    "sequence": 12,
    "title": "郊庙歌辞·周郊祀乐章·感顺乐",
    "author": "佚名",
    "content": [
      "明君陈大礼，展币祀圆丘。",
      "雅乐声齐发，祥云色正浮。"
    ]
  },
  {
    "volume": 16,
    "sequence": 13,
    "title": "郊庙歌辞·周郊祀乐章·禋顺乐",
    "author": "佚名",
    "content": [
      "黄彝将献，特牲预迎。",
      "既修昭事，潜达明诚。"
    ]
  },
  {
    "volume": 16,
    "sequence": 14,
    "title": "郊庙歌辞·周郊祀乐章·福顺乐",
    "author": "佚名",
    "content": [
      "相承五运，取法三才。",
      "大礼爰展，率土咸来。",
      "卿云秘室，甘泉宝台。",
      "象樽初酌，受福不回。"
    ]
  },
  {
    "volume": 16,
    "sequence": 15,
    "title": "郊庙歌辞·周郊祀乐章·福顺乐",
    "author": "佚名",
    "content": [
      "昊天成命，邦国盛仪。",
      "多士齐列，六龙载驰。",
      "坛升泰一，乐奏咸池。",
      "高明祚德，永致昌期。"
    ]
  },
  {
    "volume": 16,
    "sequence": 16,
    "title": "郊庙歌辞·周郊祀乐章·福顺乐",
    "author": "佚名",
    "content": [
      "上天垂景贶，哲后举鸾觞。",
      "明德今方祚，邦家万世昌。"
    ]
  },
  {
    "volume": 16,
    "sequence": 17,
    "title": "郊庙歌辞·周郊祀乐章·忠顺乐",
    "author": "佚名",
    "content": [
      "木铎敷音文德昌，朱干成列武功彰。",
      "雷鼗鹭羽今休用，玉戚相参正发扬。"
    ]
  },
  {
    "volume": 16,
    "sequence": 18,
    "title": "郊庙歌辞·周郊祀乐章·武舞乐",
    "author": "佚名",
    "content": [
      "圭瓒方陈礼，干旄乃象功。",
      "成文非羽籥，猛势若罴熊。"
    ]
  },
  {
    "volume": 16,
    "sequence": 19,
    "title": "郊庙歌辞·周郊祀乐章·昭顺乐",
    "author": "佚名",
    "content": [
      "云门孤竹，苍璧黄琮。",
      "既祀天地，克配祖宗。",
      "虔修盛礼，仰答玄功。",
      "神归碧落，福降无穷。"
    ]
  },
  {
    "volume": 16,
    "sequence": 20,
    "title": "郊庙歌辞·梁太庙乐舞辞·开平舞",
    "author": "佚名",
    "content": [
      "黍稷馨，醴酉齐清。",
      "牲牷洁，金石铿。",
      "恭祀事，结皇情。",
      "神来格，歌颂声。"
    ]
  },
  {
    "volume": 16,
    "sequence": 21,
    "title": "郊庙歌辞·梁太庙乐舞辞·皇帝行",
    "author": "佚名",
    "content": [
      "莫高者天，攀跻弗克。",
      "隮天有方，累仁积德。",
      "祖宗隆之，子孙履之。",
      "配天明祀，永永孝思。"
    ]
  },
  {
    "volume": 16,
    "sequence": 22,
    "title": "郊庙歌辞·梁太庙乐舞辞·帝盥",
    "author": "佚名",
    "content": [
      "庄肃莅事，周旋礼容。",
      "祼鬯严洁，穆穆雍雍。"
    ]
  },
  {
    "volume": 16,
    "sequence": 23,
    "title": "郊庙歌辞·梁太庙乐舞辞·登歌",
    "author": "佚名",
    "content": [
      "於赫我皇，建中立极。",
      "动以武功，静以文德。",
      "昭事上帝，欢心万国。",
      "大报严禋，四海述职。"
    ]
  },
  {
    "volume": 16,
    "sequence": 24,
    "title": "郊庙歌辞·梁太庙乐舞辞·大合舞",
    "author": "佚名",
    "content": [
      "於穆皇祖，浚哲雍熙。",
      "美溢中夏，化被南陲。",
      "后稷累德，公刘创基。",
      "肇兴九庙，乐合来仪。"
    ]
  },
  {
    "volume": 16,
    "sequence": 25,
    "title": "郊庙歌辞·梁太庙乐舞辞·象功舞",
    "author": "佚名",
    "content": [
      "天地合德，睿圣昭彰。",
      "累赠太傅，俄登魏王。",
      "雄名不朽，奕叶而光。",
      "建国之兆，君临万方。"
    ]
  },
  {
    "volume": 16,
    "sequence": 26,
    "title": "郊庙歌辞·梁太庙乐舞辞·来仪舞",
    "author": "佚名",
    "content": [
      "於赫帝命，应天顺人。",
      "亭育品汇，宾礼百神。",
      "洪基永固，景命惟新。",
      "肃恭孝享，祚我生民。"
    ]
  },
  {
    "volume": 16,
    "sequence": 27,
    "title": "郊庙歌辞·梁太庙乐舞辞·昭德舞",
    "author": "佚名",
    "content": [
      "肃肃文考，源浚派长。",
      "汉称诞季，周实生昌。",
      "奄有四海，超彼百王。",
      "笙镛迭奏，礼物荧煌。"
    ]
  },
  {
    "volume": 16,
    "sequence": 28,
    "title": "郊庙歌辞·梁太庙乐舞辞·饮福",
    "author": "佚名",
    "content": [
      "戛玉拟金永颂声，檿丝孤竹和且清。",
      "灵歆醉止牺象盈，自天降福千万龄。"
    ]
  },
  {
    "volume": 16,
    "sequence": 29,
    "title": "郊庙歌辞·梁太庙乐舞辞·撤豆",
    "author": "佚名",
    "content": [
      "笙镛洋洋，庭燎煌煌。",
      "明星有烂，祝史下堂。",
      "笾豆斯撤，礼容有章。",
      "克勤克俭，无怠无荒。"
    ]
  },
  {
    "volume": 16,
    "sequence": 30,
    "title": "郊庙歌辞·梁太庙乐舞辞·送神",
    "author": "佚名",
    "content": [
      "其降无从，其往无踪。",
      "黍稷非馨，有感必通。",
      "赫奕令德，仿佛睟容。",
      "再拜慌忽，遐想昊穹。"
    ]
  },
  {
    "volume": 16,
    "sequence": 31,
    "title": "郊庙歌辞·后唐宗庙乐舞辞·昭德舞",
    "author": "佚名",
    "content": [
      "懿彼明德，赫赫煌煌。",
      "名高阃域，功著旗常。",
      "道符休泰，运叶祺祥。",
      "庆传万祀，以播耿光。"
    ]
  },
  {
    "volume": 16,
    "sequence": 32,
    "title": "郊庙歌辞·后唐宗庙乐舞辞·文明舞",
    "author": "佚名",
    "content": [
      "帝业光扬，皇图翕赫。",
      "圣德孔彰，神功不测。",
      "信及豚鱼，恩沾动植。",
      "懿范鸿名，传之万亿。"
    ]
  },
  {
    "volume": 16,
    "sequence": 33,
    "title": "郊庙歌辞·后唐宗庙乐舞辞·应天舞",
    "author": "佚名",
    "content": [
      "晋国肇兴，雄图再固。",
      "黼黻帝道，金玉王度。",
      "皇天无亲，惟德是辅。",
      "载诞英明，永光圣祚。"
    ]
  },
  {
    "volume": 16,
    "sequence": 34,
    "title": "郊庙歌辞·后唐宗庙乐舞辞·永平舞",
    "author": "佚名",
    "content": [
      "庆传瓒祚，位正瑶图。",
      "功宣四海，化被八区。",
      "静彰帝道，动合乾符。",
      "千秋万祀，永荷昭苏。"
    ]
  },
  {
    "volume": 16,
    "sequence": 35,
    "title": "郊庙歌辞·后唐宗庙乐舞辞·武成舞",
    "author": "佚名",
    "content": [
      "艰难王业，返正皇唐。",
      "先天再造，却日重光。",
      "汉绍世祖，夏资少康。",
      "功成德茂，率祀无疆。"
    ]
  },
  {
    "volume": 16,
    "sequence": 36,
    "title": "郊庙歌辞·后唐宗庙乐舞辞·雍熙舞",
    "author": "佚名",
    "content": [
      "仁君御宇，寰海谧清。",
      "运符武德，道协文明。",
      "九成式叙，百度惟贞。",
      "金门积庆，玉叶传荣。"
    ]
  },
  {
    "volume": 16,
    "sequence": 37,
    "title": "郊庙歌辞·汉宗庙乐舞辞·武德舞",
    "author": "佚名",
    "content": [
      "明明我祖，天集休明。",
      "神母夜哭，彤云昼兴。",
      "笾豆有践，管籥斯登。",
      "孝孙致告，神其降灵。"
    ]
  },
  {
    "volume": 16,
    "sequence": 38,
    "title": "郊庙歌辞·汉宗庙乐舞辞·灵长舞",
    "author": "佚名",
    "content": [
      "天降祥，汉祚昌。",
      "火炎上，水灵长。",
      "建庙社，洁蒸尝。",
      "罗钟石，俨珩璜。",
      "陈玉豆，酌金觞。",
      "气昭感，德馨香。",
      "祗洛汭，瞻晋阳。",
      "降吾祖，福穰穰。"
    ]
  },
  {
    "volume": 16,
    "sequence": 39,
    "title": "郊庙歌辞·汉宗庙乐舞辞·积善舞",
    "author": "佚名",
    "content": [
      "黍稷斯馨，祖德惟明。",
      "蛇告赤帝，龟谋大横。",
      "云行雨施，天成地平。",
      "造我家邦，斡我璿衡。",
      "陶匏在御，醍盎惟精。",
      "或戛或击，载炮载烹。",
      "饮福受胙，舞降歌迎。",
      "滔滔不竭，洪惟水行。"
    ]
  },
  {
    "volume": 16,
    "sequence": 40,
    "title": "郊庙歌辞·汉宗庙乐舞辞·显仁舞",
    "author": "佚名",
    "content": [
      "运极金行谢，天资水德隆。",
      "礼神鄜畤馆，布政未央宫。",
      "诘旦备明祀，登歌答茂功。",
      "云轩临降久，星俎荐陈丰。",
      "霭霭沉檀雾，锵锵环佩风。",
      "荧煌升藻藉，肸蚃转珠栊。",
      "尊祖咸韶备，贻孙书轨同。",
      "京坻长有积，宗社享无穷。"
    ]
  },
  {
    "volume": 16,
    "sequence": 41,
    "title": "郊庙歌辞·汉宗庙乐舞辞·章庆舞",
    "author": "佚名",
    "content": [
      "罘罳晓唱鸡人，三牲八簋斯陈。",
      "雾集瑶阶琐闼，香生绮席华茵。",
      "珠佩貂珰熠爚，羽旄干戚纷纶。",
      "酌鬯既终三献，凝旒何止千春。",
      "阿阁长栖彩凤，郊宫叠奏祥麟。",
      "赤伏英灵未泯，玄圭运祚重新。",
      "玉斝牺樽潋滟，龙旗凤辖逡巡。",
      "瞻望月游冠冕，犹疑苍野回轮。"
    ]
  },
  {
    "volume": 16,
    "sequence": 42,
    "title": "郊庙歌辞·汉宗庙乐舞辞·观德舞",
    "author": "张昭",
    "content": [
      "高庙明灵再启图，金根玉辂幸神都。",
      "巢阿丹凤衔书命，入昴飞星献宝符。",
      "正换熏弦娱赤子，忽登仙驾泣苍梧。",
      "荐樱鹤馆笳箫咽，酌鬯金楹剑佩趋。",
      "星俎云罍兼鲁礼，朱干象箾杂巴渝。",
      "氤氲龙麝交青琐，仿佛钖銮下蕊珠。",
      "荐豆奉觞亲玉几，配天合祖耀璿枢。",
      "受禧饮酒皇欢洽，仰俟余灵泰九区。"
    ]
  },
  {
    "volume": 16,
    "sequence": 43,
    "title": "郊庙歌辞·周宗庙乐舞辞·肃顺",
    "author": "佚名",
    "content": [
      "我后至孝，祗谒祖先。",
      "仰瞻庙貌，夙设宫悬。",
      "朱弦疏越，羽舞回旋。",
      "神其来格，明祀惟虔。"
    ]
  },
  {
    "volume": 16,
    "sequence": 44,
    "title": "郊庙歌辞·周宗庙乐舞辞·治顺",
    "author": "佚名",
    "content": [
      "清庙将入，衮服是依。",
      "载行载止，令色令仪。",
      "永终就养，空极孝思。",
      "瞻望如在，顾复长违。"
    ]
  },
  {
    "volume": 16,
    "sequence": 45,
    "title": "郊庙歌辞·周宗庙乐舞辞·肃雍舞",
    "author": "佚名",
    "content": [
      "周道载兴，象日之明。",
      "万邦咸庆，百谷用成。",
      "於穆圣祖，祗荐鸿名。",
      "祀于庙社，陈其牺牲。",
      "进旅退旅，皇武之形。",
      "一倡三叹，朱弦之声。",
      "以妥以侑，既和且平。",
      "至诚潜达，介福攸宁。"
    ]
  },
  {
    "volume": 16,
    "sequence": 46,
    "title": "郊庙歌辞·周宗庙乐舞辞·章德舞",
    "author": "佚名",
    "content": [
      "清庙新，展严禋。",
      "恭祖德，厚人伦。",
      "雅乐荐，礼器陈。",
      "俨皇尸，列虞宾。",
      "神如在，声不闻。",
      "享必信，貌惟夤。",
      "想龙服，奠牺樽。",
      "礼既备，庆来臻。"
    ]
  },
  {
    "volume": 16,
    "sequence": 47,
    "title": "郊庙歌辞·周宗庙乐舞辞·善庆舞",
    "author": "佚名",
    "content": [
      "卜世长，帝祚昌。",
      "定中国，服四方。",
      "修明祀，从旧章。",
      "奏激楚，转清商。",
      "罗俎豆，列簪裳。",
      "歌累累，容皇皇。",
      "望来格，降休祥。",
      "祝敢告，寿无疆。"
    ]
  },
  {
    "volume": 16,
    "sequence": 48,
    "title": "郊庙歌辞·周宗庙乐舞辞·观成舞",
    "author": "佚名",
    "content": [
      "穆穆王国，奕奕神功。",
      "毖祀载展，明德有融。",
      "彝樽斯满，簠簋斯丰。",
      "纷絺旄羽，锵洋磬钟。",
      "或升或降，克和克同。",
      "孔惠之礼，必肃之容。",
      "锡以纯嘏，祚其允恭。",
      "神保是飨，万世无穷。"
    ]
  },
  {
    "volume": 16,
    "sequence": 49,
    "title": "郊庙歌辞·周宗庙乐舞辞·明德舞",
    "author": "佚名",
    "content": [
      "惟彼岐阳，德大流光。",
      "载造周室，泽及遐荒。",
      "於铄圣祖，上帝是皇。",
      "乃圣乃神，知微知彰。",
      "新庙奕奕，丰年穰穰。",
      "取彼血膋，以往蒸尝。",
      "黍稷惟馨，笾豆大房。",
      "工祝致告，受福无疆。"
    ]
  },
  {
    "volume": 16,
    "sequence": 50,
    "title": "郊庙歌辞·周宗庙乐舞辞·咸顺",
    "author": "佚名",
    "content": [
      "万舞咸列，三阶克清。",
      "贯珠一倡，击石九成。",
      "盈觞虽酌，灵坐无形。",
      "永怀我祖，达其孝诚。"
    ]
  },
  {
    "volume": 16,
    "sequence": 51,
    "title": "郊庙歌辞·周宗庙乐舞辞·禋顺",
    "author": "佚名",
    "content": [
      "旨酒既献，嘉殽乃迎。",
      "振其鼗鼓，洁以铏羹。",
      "肇禋肇祀，或炮或烹。",
      "皇尸俨若，保飨是明。"
    ]
  },
  {
    "volume": 16,
    "sequence": 52,
    "title": "郊庙歌辞·周宗庙乐舞辞·福顺",
    "author": "佚名",
    "content": [
      "新庙奕奕，金奏洋洋。",
      "享于祖考，循彼典章。",
      "清酤特满，嘉玉腾光。",
      "神醉既告，帝祉无疆。"
    ]
  },
  {
    "volume": 16,
    "sequence": 53,
    "title": "郊庙歌辞·周宗庙乐舞辞·忠顺",
    "author": "佚名",
    "content": [
      "称文既表温柔德，示武须成蹈厉容。",
      "缀兆疾舒皆应节，明明我祖乐无穷。"
    ]
  },
  {
    "volume": 16,
    "sequence": 54,
    "title": "郊庙歌辞·善胜舞",
    "author": "佚名",
    "content": [
      "圣祖累功，福钟来裔。",
      "持羽执干。",
      "舞文不废。"
    ]
  },
  {
    "volume": 16,
    "sequence": 55,
    "title": "郊庙歌辞·善胜舞·禋顺",
    "author": "佚名",
    "content": [
      "礼毕祀先，香散几筵。",
      "罢舞干戚，收撤豆笾。"
    ]
  },
  {
    "volume": 16,
    "sequence": 56,
    "title": "郊庙歌辞·善胜舞·肃顺",
    "author": "佚名",
    "content": [
      "乐奏四顺，福受万年。",
      "神归碧天，庭余瑞烟。"
    ]
  },
  {
    "volume": 16,
    "sequence": 57,
    "title": "郊庙歌辞·晋朝飨乐章·初举酒文同乐",
    "author": "佚名",
    "content": [
      "赫矣昌运，明哉圣王。",
      "文兴坠典，礼复旧章。",
      "鸳鸾济济，鸟兽跄跄。",
      "一人有庆，万福无疆。"
    ]
  },
  {
    "volume": 16,
    "sequence": 58,
    "title": "郊庙歌辞·晋朝飨乐章·举酒",
    "author": "佚名",
    "content": [
      "大明御宇，至德动天。",
      "君臣庆会，礼乐昭宣。",
      "剑佩成列，金石在悬。",
      "椒觞再献，宝历万年。"
    ]
  },
  {
    "volume": 16,
    "sequence": 59,
    "title": "郊庙歌辞·晋朝飨乐章·再举酒",
    "author": "佚名",
    "content": [
      "朝野无事，寰瀛大康。",
      "圣人有作，盛礼重光。",
      "万国执玉，千官奉觞。",
      "南山永固，地久天长。"
    ]
  },
  {
    "volume": 16,
    "sequence": 60,
    "title": "郊庙歌辞·晋朝飨乐章·四举酒",
    "author": "佚名",
    "content": [
      "八表欢无事，三秋贺有成。",
      "照临同日远，渥泽并云行。",
      "河变千年色，山呼万岁声。",
      "愿修封岱礼，方以称文明。"
    ]
  },
  {
    "volume": 16,
    "sequence": 61,
    "title": "郊庙歌辞·晋朝飨乐章·群臣酒行歌",
    "author": "佚名",
    "content": [
      "剑佩俨如林，齐倾拱北心。",
      "渥恩颁美禄，咸濩听和音。",
      "一德君臣合，重瞳日月临。",
      "歌时兼乐圣，唯待赞泥金。",
      "万国咸归禹，千官共祝尧。",
      "拜恩瞻凤扆，倾耳听云韶。",
      "运启金行远，时和玉烛调。",
      "酒酣齐抃舞，同贺圣明朝。",
      "令节陈高会，群臣侍御筵。",
      "玉墀留爱景，金殿霭祥烟。",
      "振鹭涵天泽，灵禽下乐悬。",
      "圣明无一事，何处让尧年。"
    ]
  },
  {
    "volume": 16,
    "sequence": 62,
    "title": "郊庙歌辞·周朝飨乐章·忠顺",
    "author": "佚名",
    "content": [
      "岁迎更始，节及朝元。",
      "冕旒仰止，冠剑相连。",
      "八音合奏，万物齐宣。",
      "常陈盛礼，愿永千年。"
    ]
  },
  {
    "volume": 16,
    "sequence": 63,
    "title": "郊庙歌辞·周朝飨乐章·忠顺",
    "author": "佚名",
    "content": [
      "明君当宁，列辟奉觞。",
      "云容表瑞，日影初长。",
      "衣冠济济，钟磬洋洋。",
      "令仪克盛，嘉会有章。"
    ]
  },
  {
    "volume": 16,
    "sequence": 64,
    "title": "郊庙歌辞·周朝飨乐章·治顺",
    "author": "佚名",
    "content": [
      "庭陈大乐，坐当太微。",
      "凝旒负扆，端拱垂衣。",
      "鸳鸾成列，簪组相辉。",
      "御炉香散，郁郁霏霏。"
    ]
  },
  {
    "volume": 16,
    "sequence": 65,
    "title": "郊庙歌辞·周朝飨乐章·福顺",
    "author": "佚名",
    "content": [
      "圣皇端拱，多士输忠。",
      "蛮觞共献，臣心毕同。",
      "声齐嵩岳，祝比华封。",
      "千龄万祀，常保时雍。"
    ]
  },
  {
    "volume": 16,
    "sequence": 66,
    "title": "郊庙歌辞·周朝飨乐章·康顺",
    "author": "佚名",
    "content": [
      "鸿钧广运，嘉节良辰。",
      "列辟在位，万国来宾。",
      "干旌屡舞，金石咸陈。",
      "礼容既备，帝履长春。"
    ]
  },
  {
    "volume": 16,
    "sequence": 67,
    "title": "郊庙歌辞·周朝飨乐章·忠顺",
    "author": "佚名",
    "content": [
      "礼成三爵，乐毕九成。",
      "共离金戺，复列彤庭。"
    ]
  },
  {
    "volume": 16,
    "sequence": 68,
    "title": "郊庙歌辞·周朝飨乐章·忠顺",
    "author": "佚名",
    "content": [
      "明庭展礼，为龙为光。",
      "咸韶息韵，鹓鹭归行。"
    ]
  },
  {
    "volume": 16,
    "sequence": 69,
    "title": "郊庙歌辞·晋昭德成功舞歌·昭德舞歌二首",
    "author": "佚名",
    "content": [
      "圣代修文德，明庭举旧章。",
      "两阶陈羽籥，万舞合宫商。",
      "剑佩森鸳鹭，箫韶下凤凰。",
      "我朝青史上，千古有辉光。",
      "淮海干戈戢，朝廷礼乐施。",
      "白驹皆就絷，丹凤复来仪。",
      "德备三苗格，风行万国随。",
      "小臣同百兽，率舞贺昌期。"
    ]
  },
  {
    "volume": 16,
    "sequence": 70,
    "title": "郊庙歌辞·晋昭德成功舞歌·武功舞歌二首",
    "author": "佚名",
    "content": [
      "拨乱资英王，开基自晋阳。",
      "一戎成大业，七德焕前王。",
      "炎汉提封远，姬周世祚长。",
      "朱干将玉戚，全象武功扬。",
      "睿算超前古，神功格上圆。",
      "百川留禹迹，万国戴尧天。",
      "既已櫜弓矢，诚宜播管弦。",
      "跄跄随鸟兽，共乐太平年。"
    ]
  }
]