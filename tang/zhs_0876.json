[
  {
    "volume": 876,
    "sequence": 1,
    "title": "二贺诗",
    "author": "佚名",
    "content": [
      "学行可师贺德基，文质彬彬贺德仁。"
    ]
  },
  {
    "volume": 876,
    "sequence": 2,
    "title": "四王语",
    "author": "佚名",
    "content": [
      "宁向儋崖振白，不事江滕蒋虢。"
    ]
  },
  {
    "volume": 876,
    "sequence": 3,
    "title": "时人为屈突语",
    "author": "佚名",
    "content": [
      "宁食三斗艾，不见屈突盖。",
      "宁服三斗葱，不逢屈突通。"
    ]
  },
  {
    "volume": 876,
    "sequence": 4,
    "title": "杨刺史语",
    "author": "佚名",
    "content": [
      "宁食三斗蒜，不逢杨德幹。"
    ]
  },
  {
    "volume": 876,
    "sequence": 5,
    "title": "万年人语",
    "author": "佚名",
    "content": [
      "宁饮三斗尘，无逢权怀恩。"
    ]
  },
  {
    "volume": 876,
    "sequence": 6,
    "title": "赞皇人语",
    "author": "佚名",
    "content": [
      "太冲无兄，孝端无弟。"
    ]
  },
  {
    "volume": 876,
    "sequence": 7,
    "title": "高宗时语",
    "author": "佚名",
    "content": [
      "左相宣威沙漠，右相驰誉丹青。",
      "三馆学生放散，五台令史经明。"
    ]
  },
  {
    "volume": 876,
    "sequence": 8,
    "title": "时人为李义甫语",
    "author": "佚名",
    "content": [
      "今日巨唐年，还诛四凶族。"
    ]
  },
  {
    "volume": 876,
    "sequence": 9,
    "title": "洛州语",
    "author": "佚名",
    "content": [
      "洛州有前贾后张，可敌京兆三王。"
    ]
  },
  {
    "volume": 876,
    "sequence": 10,
    "title": "江淮间语",
    "author": "佚名",
    "content": [
      "贵如许郝，富若田彭。"
    ]
  },
  {
    "volume": 876,
    "sequence": 11,
    "title": "河北语",
    "author": "佚名",
    "content": [
      "唯此两何，杀人最多。"
    ]
  },
  {
    "volume": 876,
    "sequence": 12,
    "title": "李昭德为王弘义语",
    "author": "佚名",
    "content": [
      "昔闻苍鹰狱吏，今见白兔御史。"
    ]
  },
  {
    "volume": 876,
    "sequence": 13,
    "title": "京洛语",
    "author": "佚名",
    "content": [
      "衣裳好，仪貌恶。",
      "不姓许，即姓郝。"
    ]
  },
  {
    "volume": 876,
    "sequence": 14,
    "title": "台中语",
    "author": "佚名",
    "content": [
      "侯知一不伏致仕，张悰自请起复。",
      "高筠不肯作孝，张栖贞情愿遭忧。"
    ]
  },
  {
    "volume": 876,
    "sequence": 15,
    "title": "吏人语",
    "author": "佚名",
    "content": [
      "不畏侯卿杖，惟畏尹卿笔。"
    ]
  },
  {
    "volume": 876,
    "sequence": 16,
    "title": "选人语",
    "author": "佚名",
    "content": [
      "有钱石上好，无钱刘下好，士大夫张下好。",
      "硕学师刘子，儒生用与言。",
      "今年柿子并遭霜，为语石榴须早摘。"
    ]
  },
  {
    "volume": 876,
    "sequence": 17,
    "title": "韦氏语",
    "author": "佚名",
    "content": [
      "大郎罢相，小郎拜相。"
    ]
  },
  {
    "volume": 876,
    "sequence": 18,
    "title": "益州人吏语",
    "author": "佚名",
    "content": [
      "录事意，与天通，益州司马折威风。"
    ]
  },
  {
    "volume": 876,
    "sequence": 19,
    "title": "天授中语",
    "author": "佚名",
    "content": [
      "遇徐杜者必生，遇来侯者必死。"
    ]
  },
  {
    "volume": 876,
    "sequence": 20,
    "title": "时人为邹昉语",
    "author": "佚名",
    "content": [
      "萧佺驸马子，邹昉骆驼儿。",
      "非关道德合，只为钱相知。"
    ]
  },
  {
    "volume": 876,
    "sequence": 21,
    "title": "题张昌仪门语",
    "author": "佚名",
    "content": [
      "一两丝能得几时络。"
    ]
  },
  {
    "volume": 876,
    "sequence": 22,
    "title": "时人号李知远语",
    "author": "佚名",
    "content": [
      "李下无蹊。"
    ]
  },
  {
    "volume": 876,
    "sequence": 23,
    "title": "又号李乂语",
    "author": "佚名",
    "content": [
      "李下无蹊径。"
    ]
  },
  {
    "volume": 876,
    "sequence": 24,
    "title": "窦仆射语",
    "author": "佚名",
    "content": [
      "窦仆射，前为韦氏国父者，后作公主邑丞。"
    ]
  },
  {
    "volume": 876,
    "sequence": 25,
    "title": "时人为崔无诐语",
    "author": "佚名",
    "content": [
      "皇后嫁女，天子娶妇。"
    ]
  },
  {
    "volume": 876,
    "sequence": 26,
    "title": "神龙中语",
    "author": "佚名",
    "content": [
      "崔冉郑，乱时政。"
    ]
  },
  {
    "volume": 876,
    "sequence": 27,
    "title": "斜封官语",
    "author": "佚名",
    "content": [
      "姚宋为相，邪不如正。",
      "太平用事，正不如邪。"
    ]
  },
  {
    "volume": 876,
    "sequence": 28,
    "title": "李处郁语",
    "author": "佚名",
    "content": [
      "飧若入咽，百无一全。"
    ]
  },
  {
    "volume": 876,
    "sequence": 29,
    "title": "景云初语",
    "author": "佚名",
    "content": [
      "前有裴马，后有卢李。"
    ]
  },
  {
    "volume": 876,
    "sequence": 30,
    "title": "先天时京中语",
    "author": "佚名",
    "content": [
      "姜师度一心看地，傅孝忠两眼相天。"
    ]
  },
  {
    "volume": 876,
    "sequence": 31,
    "title": "时人号王丘、崔沔语",
    "author": "佚名",
    "content": [
      "丘山岌岌连天峻，沔水澄澄彻底清。"
    ]
  },
  {
    "volume": 876,
    "sequence": 32,
    "title": "吏部过官语",
    "author": "佚名",
    "content": [
      "麟之口，光庭手。"
    ]
  },
  {
    "volume": 876,
    "sequence": 33,
    "title": "郇公厨语",
    "author": "佚名",
    "content": [
      "人欲不饭筋骨舒，夤缘须入郇公厨。"
    ]
  },
  {
    "volume": 876,
    "sequence": 34,
    "title": "四俊语",
    "author": "佚名",
    "content": [
      "令君四俊，苗吕崔员。"
    ]
  },
  {
    "volume": 876,
    "sequence": 35,
    "title": "八友语",
    "author": "佚名",
    "content": [
      "殷颜柳陆，李萧邵赵。"
    ]
  },
  {
    "volume": 876,
    "sequence": 36,
    "title": "罗吉口号",
    "author": "佚名",
    "content": [
      "罗钳吉网。"
    ]
  },
  {
    "volume": 876,
    "sequence": 37,
    "title": "里闾诅语",
    "author": "佚名",
    "content": [
      "若违教，值三豹。"
    ]
  },
  {
    "volume": 876,
    "sequence": 38,
    "title": "陕州语",
    "author": "佚名",
    "content": [
      "不须赛神明，不必求巫祝。",
      "尔莫犯卢公，立便有祸福。"
    ]
  },
  {
    "volume": 876,
    "sequence": 39,
    "title": "真源邑语",
    "author": "佚名",
    "content": [
      "南金口，明府手。"
    ]
  },
  {
    "volume": 876,
    "sequence": 40,
    "title": "称二王语",
    "author": "佚名",
    "content": [
      "朝廷左相笔，天下右丞诗。"
    ]
  },
  {
    "volume": 876,
    "sequence": 41,
    "title": "代宗朝京师语",
    "author": "佚名",
    "content": [
      "常无分别元好钱，贤者愚，愚者贤。"
    ]
  },
  {
    "volume": 876,
    "sequence": 42,
    "title": "戏谏司语",
    "author": "佚名",
    "content": [
      "韩谏议虽分左右，归拾遗莫辨存亡。"
    ]
  },
  {
    "volume": 876,
    "sequence": 43,
    "title": "裴度语",
    "author": "佚名",
    "content": [
      "鸡猪鱼蒜，逢著则吃。",
      "生老病死，时至则行。"
    ]
  },
  {
    "volume": 876,
    "sequence": 44,
    "title": "魏博语",
    "author": "佚名",
    "content": [
      "长安天子，魏府牙军。"
    ]
  },
  {
    "volume": 876,
    "sequence": 45,
    "title": "宝历宫中语",
    "author": "佚名",
    "content": [
      "宝帐香重重，一双红芙蓉。"
    ]
  },
  {
    "volume": 876,
    "sequence": 46,
    "title": "京师人号牛杨语",
    "author": "佚名",
    "content": [
      "太牢笔，少牢口，东西南北何处走。"
    ]
  },
  {
    "volume": 876,
    "sequence": 47,
    "title": "又号牛李",
    "author": "佚名",
    "content": [
      "门生故吏，不牛则李。",
      "（李谓宗闵也）。"
    ]
  },
  {
    "volume": 876,
    "sequence": 48,
    "title": "荆南语",
    "author": "佚名",
    "content": [
      "旱不苦，祷而雨。",
      "雨不愁，公出游。"
    ]
  },
  {
    "volume": 876,
    "sequence": 49,
    "title": "郑仁表自语",
    "author": "佚名",
    "content": [
      "天瑞有五色云，人瑞有郑仁表。"
    ]
  },
  {
    "volume": 876,
    "sequence": 50,
    "title": "湖苏二郡语",
    "author": "佚名",
    "content": [
      "湖接两头，苏联三尾。"
    ]
  },
  {
    "volume": 876,
    "sequence": 51,
    "title": "广明初都人语",
    "author": "佚名",
    "content": [
      "黄贼打黑贼。"
    ]
  },
  {
    "volume": 876,
    "sequence": 52,
    "title": "吏部旧语",
    "author": "佚名",
    "content": [
      "长名以前，选人属侍郎。",
      "长名以后，侍郎属选人。"
    ]
  },
  {
    "volume": 876,
    "sequence": 53,
    "title": "省中语",
    "author": "佚名",
    "content": [
      "后行祠屯，不博中行都门。",
      "中行礼部，不博前行驾库。"
    ]
  },
  {
    "volume": 876,
    "sequence": 54,
    "title": "郎吏语",
    "author": "佚名",
    "content": [
      "司门水部，入省不数。"
    ]
  },
  {
    "volume": 876,
    "sequence": 55,
    "title": "谏院台省语",
    "author": "佚名",
    "content": [
      "遗补相惜，御史相憎，郎官相轻。"
    ]
  },
  {
    "volume": 876,
    "sequence": 56,
    "title": "御史台语",
    "author": "佚名",
    "content": [
      "事长如事端。"
    ]
  },
  {
    "volume": 876,
    "sequence": 57,
    "title": "京兆府语",
    "author": "佚名",
    "content": [
      "不立两县令，不坐两少尹。"
    ]
  },
  {
    "volume": 876,
    "sequence": 58,
    "title": "翰林谏议语",
    "author": "佚名",
    "content": [
      "饶道斗上坡去，亦须却下坡来。"
    ]
  },
  {
    "volume": 876,
    "sequence": 59,
    "title": "举子语",
    "author": "佚名",
    "content": [
      "槐花黄，举子忙。"
    ]
  },
  {
    "volume": 876,
    "sequence": 60,
    "title": "明经进士语",
    "author": "佚名",
    "content": [
      "三十老明经，五十少进士。"
    ]
  },
  {
    "volume": 876,
    "sequence": 61,
    "title": "杂帖语",
    "author": "佚名",
    "content": [
      "常杂鲍帖。"
    ]
  },
  {
    "volume": 876,
    "sequence": 62,
    "title": "闽人语",
    "author": "佚名",
    "content": [
      "欧阳独步，藻蕴横行。"
    ]
  },
  {
    "volume": 876,
    "sequence": 63,
    "title": "举场语",
    "author": "佚名",
    "content": [
      "欲入举场，先问苏张。",
      "苏张犹可，三杨杀我。"
    ]
  },
  {
    "volume": 876,
    "sequence": 64,
    "title": "大中后进士语",
    "author": "佚名",
    "content": [
      "欲得命通，问瑝嵎都雍。"
    ]
  },
  {
    "volume": 876,
    "sequence": 65,
    "title": "大中时语",
    "author": "佚名",
    "content": [
      "郑杨段薛，炙手可热。",
      "欲得命通，鲁绍瑰蒙。"
    ]
  },
  {
    "volume": 876,
    "sequence": 66,
    "title": "选举人语",
    "author": "佚名",
    "content": [
      "王凝裴瓚。",
      "舍弟安潜。",
      "朝中无呼字，知闻厅里，绝脱靴宾客。"
    ]
  },
  {
    "volume": 876,
    "sequence": 67,
    "title": "科目举人语",
    "author": "佚名",
    "content": [
      "未见王窦，徒劳漫走。"
    ]
  },
  {
    "volume": 876,
    "sequence": 68,
    "title": "戏杜审权语",
    "author": "佚名",
    "content": [
      "座主审权，门生处权。"
    ]
  },
  {
    "volume": 876,
    "sequence": 69,
    "title": "崔沆放榜时人语",
    "author": "佚名",
    "content": [
      "座主门生，沆瀣一家。"
    ]
  },
  {
    "volume": 876,
    "sequence": 70,
    "title": "唐末五代人语",
    "author": "佚名",
    "content": [
      "及第不必读书，作官何须事业。"
    ]
  },
  {
    "volume": 876,
    "sequence": 71,
    "title": "号沈宋语",
    "author": "佚名",
    "content": [
      "苏李居前，沈宋比肩。"
    ]
  },
  {
    "volume": 876,
    "sequence": 72,
    "title": "号钱郎语",
    "author": "佚名",
    "content": [
      "前有沈宋，后有钱郎。"
    ]
  },
  {
    "volume": 876,
    "sequence": 73,
    "title": "潘何诗赋语",
    "author": "佚名",
    "content": [
      "潘纬十年吟古镜，何涓一夜赋潇湘。"
    ]
  },
  {
    "volume": 876,
    "sequence": 74,
    "title": "魏薛草书语",
    "author": "佚名",
    "content": [
      "前有虞褚，后有薛魏。"
    ]
  },
  {
    "volume": 876,
    "sequence": 75,
    "title": "薛稷书语",
    "author": "佚名",
    "content": [
      "买褚得薛不落节。"
    ]
  },
  {
    "volume": 876,
    "sequence": 76,
    "title": "时人为刘毕语",
    "author": "佚名",
    "content": [
      "刘郎中松树孤标，毕庶子松根绝妙。"
    ]
  },
  {
    "volume": 876,
    "sequence": 77,
    "title": "时人为黄筌语",
    "author": "佚名",
    "content": [
      "黄筌画鹤，薛稷减价。"
    ]
  },
  {
    "volume": 876,
    "sequence": 78,
    "title": "时人为杨惠之语",
    "author": "佚名",
    "content": [
      "道子画，惠之塑，夺得僧繇神笔路。"
    ]
  },
  {
    "volume": 876,
    "sequence": 79,
    "title": "道杰语",
    "author": "佚名",
    "content": [
      "大头杰，难杀人。"
    ]
  },
  {
    "volume": 876,
    "sequence": 80,
    "title": "马周疏引俚语",
    "author": "佚名",
    "content": [
      "贫不学俭，富不学奢。"
    ]
  },
  {
    "volume": 876,
    "sequence": 81,
    "title": "杜甫引俚语",
    "author": "佚名",
    "content": [
      "城南韦杜，去天尺五。"
    ]
  },
  {
    "volume": 876,
    "sequence": 82,
    "title": "孙光宪《琐言》引古语",
    "author": "佚名",
    "content": [
      "乘船走马，去死一分。",
      "好事不出门，恶事行千里。"
    ]
  },
  {
    "volume": 876,
    "sequence": 83,
    "title": "杜重威引俚语",
    "author": "佚名",
    "content": [
      "逢贼得命，更望复子。"
    ]
  },
  {
    "volume": 876,
    "sequence": 84,
    "title": "建安语",
    "author": "佚名",
    "content": [
      "龙门一半在闽川。"
    ]
  },
  {
    "volume": 876,
    "sequence": 85,
    "title": "江陵语",
    "author": "佚名",
    "content": [
      "琵琶多于饭甑，措大多于鲫鱼。"
    ]
  },
  {
    "volume": 876,
    "sequence": 86,
    "title": "汾晋间语",
    "author": "佚名",
    "content": [
      "欲作千箱主，问取黄金母。"
    ]
  },
  {
    "volume": 876,
    "sequence": 87,
    "title": "秦中儿童语",
    "author": "佚名",
    "content": [
      "颠当牢守门，蠮螉寇汝无处奔。"
    ]
  },
  {
    "volume": 876,
    "sequence": 88,
    "title": "沧洲语",
    "author": "佚名",
    "content": [
      "不戴金莲花，不得到仙家。"
    ]
  },
  {
    "volume": 876,
    "sequence": 89,
    "title": "段成式《酉阳杂俎》引古语",
    "author": "佚名",
    "content": [
      "三守庚申三尸伏，七守庚申七尺灭。"
    ]
  },
  {
    "volume": 876,
    "sequence": 90,
    "title": "佛书引语",
    "author": "佚名",
    "content": [
      "停囚长智。",
