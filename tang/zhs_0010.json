[
  {
    "volume": 10,
    "sequence": 1,
    "title": "郊庙歌辞·祀圜丘乐章·豫和",
    "author": "佚名",
    "content": [
      "上灵眷命膺会昌，盛德殷荐叶辰良。",
      "景福降兮圣德远，玄化穆兮天历长。"
    ]
  },
  {
    "volume": 10,
    "sequence": 2,
    "title": "郊庙歌辞·祀圜丘乐章·太和",
    "author": "佚名",
    "content": [
      "穆穆我后，道应千龄。",
      "登三处大，得一居贞。",
      "礼惟崇德，乐以和声。",
      "百神仰止，天下文明。"
    ]
  },
  {
    "volume": 10,
    "sequence": 3,
    "title": "郊庙歌辞·祀圜丘乐章·肃和",
    "author": "佚名",
    "content": [
      "闓阳播气，甄曜垂明。",
      "有赫圜宰，深仁曲成。",
      "日丽苍璧，烟开紫营。",
      "聿遵乾享，式降鸿祯。"
    ]
  },
  {
    "volume": 10,
    "sequence": 4,
    "title": "郊庙歌辞·祀圜丘乐章·雍和",
    "author": "佚名",
    "content": [
      "钦惟大帝，载仰皇穹。",
      "始命田烛，爰启郊宫。",
      "云门骇听，雷鼓鸣空。",
      "神其介祀，景祚斯融。"
    ]
  },
  {
    "volume": 10,
    "sequence": 5,
    "title": "郊庙歌辞·祀圜丘乐章·寿和",
    "author": "佚名",
    "content": [
      "八音斯奏，三献毕陈。",
      "宝祚惟永，晖光日新。"
    ]
  },
  {
    "volume": 10,
    "sequence": 6,
    "title": "郊庙歌辞·祀圜丘乐章·舒和",
    "author": "佚名",
    "content": [
      "叠璧凝影皇坛路，编珠流彩帝郊前。",
      "已奏黄钟歌大吕，还符宝历祚昌年。"
    ]
  },
  {
    "volume": 10,
    "sequence": 7,
    "title": "郊庙歌辞·祀圜丘乐章·凯安",
    "author": "佚名",
    "content": [
      "昔在炎运终，中华乱无象。",
      "酆郊赤乌见，邙山黑云上。",
      "大资下周车，禁暴开殷网。",
      "幽明同叶赞，鼎祚齐天壤。"
    ]
  },
  {
    "volume": 10,
    "sequence": 8,
    "title": "郊庙歌辞·祀圜丘乐章·豫和",
    "author": "佚名",
    "content": [
      "歌奏毕兮礼献终，六龙驭兮神将升。",
      "明德感兮非黍稷，降福简兮祚休征。"
    ]
  },
  {
    "volume": 10,
    "sequence": 9,
    "title": "郊庙歌辞·郊天旧乐章·豫和",
    "author": "佚名",
    "content": [
      "蘋蘩礼著，黍稷诚微。",
      "音盈凤管，彩驻龙旗。",
      "洪歆式就，介福攸归。",
      "送乐有阕，灵驭遄飞。"
    ]
  },
  {
    "volume": 10,
    "sequence": 10,
    "title": "郊庙歌辞·武后大享昊天乐章",
    "author": "佚名",
    "content": [
      "大阴凝至化，真耀蕴轩仪。",
      "德迈娥台敞，仁高姒幄披。",
      "扪天遂启极，梦日乃升曦。",
      "瞻紫极，望玄穹。",
      "翘至恳，罄深衷。",
      "听虽远，诚必通。",
      "垂厚泽，降云宫。",
      "乾仪混成冲邃，天道下济高明。",
      "闓阳晨披紫阙，太一晓降黄庭。",
      "圆坛敢由昭报，方璧冀展虔情。",
      "丹襟式敷衷恳，玄鉴庶察微诚。",
      "巍巍睿业广，赫赫圣基隆。",
      "菲德承先顾，祯符萃眇躬。",
      "铭开武岩侧，图荐洛川中。",
      "微诚讵幽感，景命忽昭融。",
      "有怀惭紫极，无以谢玄穹。",
      "朝坛雾卷，曙岭烟沉。",
      "爰设筐币，式表诚心。",
      "筵辉丽璧，乐畅和音。",
      "仰惟灵鉴，俯察翘襟。",
      "昭昭上帝，穆穆下临。",
      "礼崇备物，乐奏锵金。",
      "兰羞委荐，桂醑盈斟。",
      "敢希明德，聿罄庄心。",
      "樽浮九酝，礼备三周。",
      "陈诚菲奠，契福神猷。",
      "奠璧郊坛昭大礼，锵金拊石表虔诚。",
      "始奏承云娱帝赏，复歌调露畅韶英。",
      "荷恩承顾托，执契恭临抚。",
      "庙略静边荒，天兵耀神武。",
      "有截资先化，无为遵旧矩。",
      "祯符降昊穹，大业光寰宇。",
      "肃肃祀典，邕邕礼秩。",
      "三献已周，九成斯毕。",
      "爰撤其俎，载迁其实。",
      "或升或降，唯诚唯质。",
      "礼终肆类，乐阕九成。",
      "仰惟明德，敢荐非馨。",
      "顾惭菲奠，久驻云輧。",
      "瞻荷灵泽，悚恋兼盈。",
      "式乾路，辟天扉。",
      "回日驭，动云衣。",
      "登金阙，入紫微。",
      "望仙驾，仰恩徽。"
    ]
  },
  {
    "volume": 10,
    "sequence": 11,
    "title": "中宗祀昊天乐章·豫和",
    "author": "佚名",
    "content": [
      "天之历数归睿唐，顾惟菲德钦昊苍。",
      "撰吉日兮表殷荐，冀神鉴兮降闓阳。"
    ]
  },
  {
    "volume": 10,
    "sequence": 12,
    "title": "中宗祀昊天乐章·太和",
    "author": "佚名",
    "content": [
      "恭临宝位，肃奉瑶图。",
      "恒思解网，每轸泣辜。",
      "德惭巢燧，化劣唐虞。",
      "期我良弼，式赞嘉谟。"
    ]
  },
  {
    "volume": 10,
    "sequence": 13,
    "title": "中宗祀昊天乐章·告谢",
    "author": "佚名",
    "content": [
      "得一流玄泽，通三御紫宸。",
      "远叶千龄运，遐销九域尘。",
      "绝瑞骈阗集，殊祥络绎臻。",
      "登年庆栖亩，稔岁贺盈囷。"
    ]
  },
  {
    "volume": 10,
    "sequence": 14,
    "title": "中宗祀昊天乐章·肃和",
    "author": "佚名",
    "content": [
      "悠哉广覆，大矣曲成。",
      "九玄著象，七曜贞明。",
      "珪璧是奠，酝酎斯盈。",
      "作乐崇德，爰畅咸英。"
    ]
  },
  {
    "volume": 10,
    "sequence": 15,
    "title": "中宗祀昊天乐章·雍和",
    "author": "佚名",
    "content": [
      "郊坛展敬，严配因心。",
      "孤竹箫管，空桑瑟琴。",
      "肃穆大礼，铿锵八音。",
      "恭惟上帝，希降灵歆。"
    ]
  },
  {
    "volume": 10,
    "sequence": 16,
    "title": "中宗祀昊天乐章·福和",
    "author": "佚名",
    "content": [
      "九成爰奏，三献式陈。",
      "钦承景福，恭托明禋。"
    ]
  },
  {
    "volume": 10,
    "sequence": 17,
    "title": "中宗祀昊天乐章·中宫助祭升坛",
    "author": "佚名",
    "content": [
      "坤元光至德，柔训阐皇风。",
      "芣苡芳声远，螽斯美化隆。",
      "睿范超千载，嘉猷备六宫。",
      "肃恭陪盛典，钦若荐禋宗。"
    ]
  },
  {
    "volume": 10,
    "sequence": 18,
    "title": "中宗祀昊天乐章·亚献",
    "author": "佚名",
    "content": [
      "三灵降飨，三后配神。",
      "虔敷藻奠，敬展郊禋。"
    ]
  },
  {
    "volume": 10,
    "sequence": 19,
    "title": "中宗祀昊天乐章·舒和",
    "author": "佚名",
    "content": [
      "已陈粢盛敷严祀，更奏笙镛协雅声。",
      "璇图宝历欣宁谧，晏俗淳风乐太平。"
    ]
  },
  {
    "volume": 10,
    "sequence": 20,
    "title": "中宗祀昊天乐章·凯安",
    "author": "佚名",
    "content": [
      "堂堂圣祖兴，赫赫昌基泰。",
      "戎车盟津偃，玉帛涂山会。",
      "舜日启祥晖，尧云卷征旆。",
      "风猷被有截，声教覃无外。"
    ]
  },
  {
    "volume": 10,
    "sequence": 21,
    "title": "明皇祀圜丘乐章·豫和",
    "author": "佚名",
    "content": [
      "至矣丕构，烝哉太平。",
      "授牺膺箓，复禹继明。",
      "草木仁化，凫鹥颂声。",
      "祀宗陈德，无愧斯诚。"
    ]
  },
  {
    "volume": 10,
    "sequence": 22,
    "title": "明皇祀圜丘乐章·太和",
    "author": "佚名",
    "content": [
      "郊坛斋帝，礼乐祀天。",
      "丹青寰宇，宫征山川。",
      "神祇毕降，行止重旋。",
      "融融穆穆，纳祉洪延。"
    ]
  },
  {
    "volume": 10,
    "sequence": 23,
    "title": "明皇祀圜丘乐章·肃和",
    "author": "佚名",
    "content": [
      "止奏潜聆，登仪宿啭。",
      "太玉躬奉，参钟首奠。",
      "簠簋聿升，牺牲递荐。",
      "昭事颙若，存存以伣。"
    ]
  },
  {
    "volume": 10,
    "sequence": 24,
    "title": "明皇祀圜丘乐章·雍和",
    "author": "佚名",
    "content": [
      "烂云普洽，律风无外。",
      "千品其凝，九宾斯会。",
      "禋尊晋烛，纯牺涤汰。",
      "玄覆攸广，鸿休汪濊。"
    ]
  },
  {
    "volume": 10,
    "sequence": 25,
    "title": "明皇祀圜丘乐章·寿和",
    "author": "佚名",
    "content": [
      "六变爰阕，八阶载虔。",
      "祐我皇祚，于万斯年。"
    ]
  },
  {
    "volume": 10,
    "sequence": 26,
    "title": "明皇祀圜丘乐章·寿和",
    "author": "佚名",
    "content": [
      "於赫圣祖，龙飞晋阳。",
      "底定万国，奄有四方。",
      "功格上下，道冠农黄。",
      "郊天配享，德合无疆。"
    ]
  },
  {
    "volume": 10,
    "sequence": 27,
    "title": "明皇祀圜丘乐章·寿和",
    "author": "佚名",
    "content": [
      "崇崇泰畤，肃肃严禋。",
      "粢盛既洁，金石毕陈。",
      "上帝来享，介福爰臻。",
      "受釐合祉，宝祚维新。"
    ]
  },
  {
    "volume": 10,
    "sequence": 28,
    "title": "明皇祀圜丘乐章·舒和",
    "author": "佚名",
    "content": [
      "祝史正辞，人神叶庆。",
      "福以德招，享以诚应。",
      "六变云备，百礼期浃。",
      "祀事孔明。",
      "祚流万叶。"
    ]
  },
  {
    "volume": 10,
    "sequence": 29,
    "title": "明皇祀圜丘乐章·凯安",
    "author": "佚名",
    "content": [
      "馨香惟后德，明命光天保。",
      "肃和崇圣灵，陈信表皇道。",
      "玉鏚初蹈万，金匏既静好。"
    ]
  },
  {
    "volume": 10,
    "sequence": 30,
    "title": "明皇祀圜丘乐章·豫和",
    "author": "佚名",
    "content": [
      "大号成命，思文配天。",
      "神光肸蚃，龙驾言旋。",
      "眇眇阊阖，昭昭上玄。",
      "俾昌而大。",
      "于斯万年。"
    ]
  },
  {
    "volume": 10,
    "sequence": 31,
    "title": "明皇祀圜丘乐章·太和",
    "author": "佚名",
    "content": [
      "六成既阕，三荐云终。",
      "神心具醉，圣敬愈崇。",
      "受釐皇邸，回跸帷宫。",
      "穰穰之福，永永无穷。"
    ]
  },
  {
    "volume": 10,
    "sequence": 32,
    "title": "封泰山乐章·豫和",
    "author": "佚名",
    "content": [
      "款泰坛，柴泰清。",
      "受天命，报天成。",
      "竦皇心，荐乐声。",
      "志上达，歌下迎。",
      "亿上帝，临下庭。",
      "骑日月，陪列星。",
      "嘉祝信，大糦馨。",
      "澹神心，醉皇灵。",
      "相百辟，贡八荒。",
      "九歌叙，万舞翔。",
      "肃振振，铿皇皇。",
      "帝欣欣，福穰穰。",
      "高在上，道光明。",
      "物资始，德难名。",
      "承眷命，牧苍生。",
      "寰宇谧，太阶平。",
      "天道无亲，至诚与邻。",
      "山川遍礼，宫徵维新。",
      "玉帛非盛，聪明会真。",
      "正斯一德，通乎百神。",
      "飨帝飨亲，维孝维圣。",
      "缉熙懿德，敷扬成命。",
      "华夷志同，笙镛礼盛。",
      "明灵降止，感此诚敬。"
    ]
  },
  {
    "volume": 10,
    "sequence": 33,
    "title": "封泰山乐章·太和",
    "author": "佚名",
    "content": [
      "孝敬中发，和容外彰。",
      "腾华照宇，如升太阳。",
      "贞璧就奠，玄灵垂光。",
      "礼乐具举，济济洋洋。"
    ]
  },
  {
    "volume": 10,
    "sequence": 34,
    "title": "封泰山乐章·肃和",
    "author": "佚名",
    "content": [
      "奠祖配天，承天享帝。",
      "百灵咸秩，四海来祭。",
      "植我苍璧，布我玄制。",
      "华日裴回，神烟容裔。"
    ]
  },
  {
    "volume": 10,
    "sequence": 35,
    "title": "封泰山乐章·雍和",
    "author": "佚名",
    "content": [
      "俎豆有馥，粢盛洁丰。",
      "亦有和羹，既戒既平。",
      "钟鼓管磬，肃唱和鸣。",
      "皇皇后祖，来我思成。"
    ]
  },
  {
    "volume": 10,
    "sequence": 36,
    "title": "封泰山乐章·寿和",
    "author": "佚名",
    "content": [
      "烝烝我后，享献惟夤。",
      "躬酌郁鬯，跪奠明神。",
      "孝莫孝乎，配上帝亲。",
      "敬莫敬乎，孝天下臣。"
    ]
  },
  {
    "volume": 10,
    "sequence": 37,
    "title": "封泰山乐章·寿和",
    "author": "佚名",
    "content": [
      "皇祖严配，配享皇天。",
      "皇皇降嘏，天子万年。"
    ]
  },
  {
    "volume": 10,
    "sequence": 38,
    "title": "封泰山乐章·舒和",
    "author": "佚名",
    "content": [
      "六钟翕协六变成，八佾倘佯八风生。",
      "乐九韶兮人神感，美七德兮天地清。"
    ]
  },
  {
    "volume": 10,
    "sequence": 39,
    "title": "封泰山乐章·凯安",
    "author": "佚名",
    "content": [
      "烈祖顺三灵，文宗威四海。",
      "黄钺诛群盗，朱旗扫多罪。",
      "戢兵天下安，约法人心改。",
      "大哉干羽意，长见风云在。"
    ]
  },
  {
    "volume": 10,
    "sequence": 40,
    "title": "封泰山乐章·豫和",
    "author": "佚名",
    "content": [
      "乐已终，烟燎上。",
      "怀灵惠，结皇想。",
      "归风疾，回风爽。",
      "百福来，众神往。"
    ]
  },
  {
    "volume": 10,
    "sequence": 41,
    "title": "祈谷乐章·肃和",
    "author": "佚名",
    "content": [
      "履艮斯绳，居中体正。",
      "龙运垂祉，昭符启圣。",
      "式事严禋，聿怀嘉庆。",
      "惟帝永锡，时皇休命。"
    ]
  },
  {
    "volume": 10,
    "sequence": 42,
    "title": "祈谷乐章·雍和",
    "author": "佚名",
    "content": [
      "殷荐乘春，太坛临曙。",
      "八簋盈和，六瑚登御。",
      "嘉稷匪歆，德馨斯饫。",
      "祝嘏无易，灵心有豫。"
    ]
  },
  {
    "volume": 10,
    "sequence": 43,
    "title": "祈谷乐章·舒和",
    "author": "佚名",
    "content": [
      "玉帛牺牲申敬享，金丝戚羽盛音容。",
      "庶俾亿龄禔景福，长欣万宇洽时邕。"
    ]
  },
  {
    "volume": 10,
    "sequence": 44,
    "title": "明堂乐章·肃和",
    "author": "佚名",
    "content": [
      "象天御宇，乘时布政。",
      "严配申虔，宗禋展敬。",
      "樽罍盈列，树羽交映。",
      "玉币通诚，祚隆皇圣。"
    ]
  },
  {
    "volume": 10,
    "sequence": 45,
    "title": "明堂乐章·雍和",
    "author": "佚名",
    "content": [
      "八牖晨披，五精朝奠。",
      "雾凝璇篚，风清金县。",
      "神涤备全，明粢丰衍。",
      "载絜彝俎，陈诚以荐。"
    ]
  },
  {
    "volume": 10,
    "sequence": 46,
    "title": "明堂乐章·舒和",
    "author": "佚名",
    "content": [
      "御扆合宫承宝历，席图重馆奉明灵。",
      "偃武修文九围泰，沉烽静柝八荒宁。"
    ]
  },
  {
    "volume": 10,
    "sequence": 47,
    "title": "武后明堂乐章·外办将出",
    "author": "佚名",
    "content": [
      "总章陈昔典，衢室礼惟神。",
      "宏规则天地，神用叶陶钧。",
      "负扆三春旦，充庭万宇宾。",
      "顾己诚虚薄，空惭亿兆人。"
    ]
  },
  {
    "volume": 10,
    "sequence": 48,
    "title": "武后明堂乐章·皇帝行",
    "author": "佚名",
    "content": [
      "仰膺历数，俯顺讴歌。",
      "远安迩肃，俗阜时和。",
      "化光玉镜，讼息金科。",
      "方兴典礼，永戢干戈。"
    ]
  },
  {
    "volume": 10,
    "sequence": 49,
    "title": "武后明堂乐章·皇嗣出入升降",
    "author": "佚名",
    "content": [
      "至人光俗，大孝通神。",
      "谦以表性，恭惟立身。",
      "洪规载启，茂典方陈。",
      "誉隆三善，祥开万春。"
    ]
  },
  {
    "volume": 10,
    "sequence": 50,
    "title": "武后明堂乐章·迎送王公",
    "author": "佚名",
    "content": [
      "千官肃事，万国朝宗。",
      "载延百辟，爰集三宫。",
      "君臣德合，鱼水斯同。",
      "睿图方永，周历长隆。"
    ]
  },
  {
    "volume": 10,
    "sequence": 51,
    "title": "武后明堂乐章·登歌",
    "author": "佚名",
    "content": [
      "礼崇宗祀，志表严禋。",
      "笙镛合奏，文物维新。",
      "敬遵茂典，敢择良辰。",
      "絜诚斯著，奠谒方申。"
    ]
  },
  {
    "volume": 10,
    "sequence": 52,
    "title": "武后明堂乐章·配飨",
    "author": "佚名",
    "content": [
      "笙镛间玉宇，文物昭清晖。",
      "睟影临芳奠，休光下太微。",
      "孝思期有感，明絜庶无违。"
    ]
  },
  {
    "volume": 10,
    "sequence": 53,
    "title": "武后明堂乐章·宫音",
    "author": "佚名",
    "content": [
      "履艮包群望，居中冠百灵。",
      "万方资广运，庶品荷财成。",
      "神功谅匪测，盛德实难名。",
      "藻奠申诚敬，恭祀表惟馨。"
    ]
  },
  {
    "volume": 10,
    "sequence": 54,
    "title": "武后明堂乐章·角音",
    "author": "佚名",
    "content": [
      "出震位，开平秩。",
      "扇条风，乘甲乙。",
      "龙德盛，鸟星出。",
      "荐珪篚，陈诚实。"
    ]
  },
  {
    "volume": 10,
    "sequence": 55,
    "title": "武后明堂乐章·徵音",
    "author": "佚名",
    "content": [
      "赫赫离精御炎陆，滔滔炽景开隆暑。",
      "冀延神鉴俯兰樽，式表虔襟陈桂俎。"
    ]
  },
  {
    "volume": 10,
    "sequence": 56,
    "title": "武后明堂乐章·商音",
    "author": "佚名",
    "content": [
      "律中夷则，序应收成。",
      "功宣建武，义表惟明。",
      "爰申礼奠，庶展翘诚。",
      "九秋是式，百谷斯盈。"
    ]
  },
  {
    "volume": 10,
    "sequence": 57,
    "title": "武后明堂乐章·羽音",
    "author": "佚名",
    "content": [
      "葭律肇启隆冬，蘋藻攸陈飨祭。",
      "黄钟既陈玉烛，红粒方殷稔岁。"
    ]
  },
  {
    "volume": 10,
    "sequence": 58,
    "title": "雩祀乐章·肃和",
    "author": "佚名",
    "content": [
      "朱鸟开辰，苍龙启映。",
      "大帝昭飨，群生展敬。",
      "礼备怀柔，功宣舞咏。",
      "旬液应序，年祥协庆。"
    ]
  },
  {
    "volume": 10,
    "sequence": 59,
    "title": "雩祀乐章·雍和",
    "author": "佚名",
    "content": [
      "绀筵分彩，宝图吐绚。",
      "风管晨凝，云歌晓啭。",
      "肃事兰羞，虔申桂奠。",
      "百谷斯登，万箱攸荐。"
    ]
  },
  {
    "volume": 10,
    "sequence": 60,
    "title": "雩祀乐章·舒和",
    "author": "佚名",
    "content": [
      "凤曲登歌调令序，龙雩集舞泛祥风。",
      "彩旞云回昭睿德，朱干电发表神功。"
    ]
  },
  {
    "volume": 10,
    "sequence": 61,
    "title": "雩祀乐章·豫和",
    "author": "佚名",
    "content": [
      "鸟纬迁序，龙星见辰。",
      "纯阳在律，明德崇禋。",
      "五方降帝，万宇安人。",
      "恭以致享，肃以迎神。"
    ]
  },
  {
    "volume": 10,
    "sequence": 62,
    "title": "雩祀乐章·豫和",
    "author": "佚名",
    "content": [
      "祀遵经设，享缘成举。",
      "献毕于樽，彻临于俎。",
      "舞止干戚，乐停枧敔。",
      "歌以送神，神还其所。"
    ]
  }
]