[
  {
    "volume": 880,
    "sequence": 1,
    "title": "城阳公主卜繇",
    "author": "佚名",
    "content": [
      "二火皆食，始同荣，末同戚。"
    ]
  },
  {
    "volume": 880,
    "sequence": 2,
    "title": "冯存澄为明皇占",
    "author": "佚名",
    "content": [
      "合因斩关，铸印乘轩。",
      "始当果断，终得嗣天。"
    ]
  },
  {
    "volume": 880,
    "sequence": 3,
    "title": "钱知微卜卖天津桥",
    "author": "佚名",
    "content": [
      "两头点土，中心虚悬。",
      "人足踏跋，不肯下钱。"
    ]
  },
  {
    "volume": 880,
    "sequence": 4,
    "title": "王山人为李卫公按冥",
    "author": "佚名",
    "content": [
      "位极人臣，寿六十四。"
    ]
  },
  {
    "volume": 880,
    "sequence": 5,
    "title": "钟传客占历日包橘",
    "author": "佚名",
    "content": [
      "太岁当头坐，诸神不敢当。",
      "其中有一物，常带洞庭香。"
    ]
  },
  {
    "volume": 880,
    "sequence": 6,
    "title": "马重绩占随卦繇辞",
    "author": "佚名",
    "content": [
      "南瞻析木，木不自续。",
      "虚而动之，动随其覆。"
    ]
  },
  {
    "volume": 880,
    "sequence": 7,
    "title": "方龟精为钱元懿卜词",
    "author": "佚名",
    "content": [
      "太乙接天河，金华宝贝多。",
      "郡侯六十六，别处不经过。"
    ]
  },
  {
    "volume": 880,
    "sequence": 8,
    "title": "叶简占失牛",
    "author": "佚名",
    "content": [
      "占失牛，已被家边载上州。",
      "欲知贼姓一斤求，欲知贼名十干头。",
      "（果邻人丘甲盗之）"
    ]
  },
  {
    "volume": 880,
    "sequence": 9,
    "title": "又射覆橘子",
    "author": "佚名",
    "content": [
      "圆似珠，色如丹。",
      "傥能擘破同分吃，争不惭愧洞庭山。"
    ]
  },
  {
    "volume": 880,
    "sequence": 10,
    "title": "射覆巾子",
    "author": "佚名",
    "content": [
      "近来好裹束，各自竞尖新。",
      "秤无三五两，因何号一斤。"
    ]
  },
  {
    "volume": 880,
    "sequence": 11,
    "title": "射覆二鸡子",
    "author": "佚名",
    "content": [
      "此物不难知，一雄兼一雌。",
      "谁将打破看，方明混沌时。"
    ]
  },
  {
    "volume": 880,
    "sequence": 12,
    "title": "天冲星占",
    "author": "佚名",
    "content": [
      "天冲抱极泣帝前，血浊雾下天下冤。"
    ]
  },
  {
    "volume": 880,
    "sequence": 13,
    "title": "占月语",
    "author": "佚名",
    "content": [
      "月如弯弓，少雨多风。",
      "月如仰瓦，不求自下。"
    ]
  },
  {
    "volume": 880,
    "sequence": 14,
    "title": "占雨",
    "author": "佚名",
    "content": [
      "乾星照湿土，明日依旧雨。",
      "云行西，星照泥。",
      "朝霞不出门，暮霞行千里。",
      "天将雨，鸠逐妇。"
    ]
  },
  {
    "volume": 880,
    "sequence": 15,
    "title": "占四时甲子雨",
    "author": "佚名",
    "content": [
      "春雨甲子，赤地千里。",
      "夏雨甲子，乘船入市。",
      "秋雨甲子，禾头生耳。",
      "冬雨甲子，牛羊冻死。",
      "鹊巢下地，其年大水。"
    ]
  },
  {
    "volume": 880,
    "sequence": 16,
    "title": "占年",
    "author": "佚名",
    "content": [
      "要见麦，见三白。",
      "正月三白，田公笑赫赫。"
    ]
  },
  {
    "volume": 880,
    "sequence": 17,
    "title": "树稼谚",
    "author": "佚名",
    "content": [
      "树稼，达官怕。"
    ]
  },
  {
    "volume": 880,
    "sequence": 18,
    "title": "相书语（张璟藏论妇人相引此）",
    "author": "佚名",
    "content": [
      "目有四白，五夫守宅。"
    ]
  },
  {
    "volume": 880,
    "sequence": 19,
    "title": "葬书语",
    "author": "佚名",
    "content": [
      "葬压龙角，其棺必斫。",
      "朱雀和鸣，子孙盛荣。",
      "朱雀悲哀，棺中见灰。",
      "安龙头，枕龙角。",
      "不三年，自消铄。",
      "安龙头，枕龙耳。",
      "不三年，万乘至。"
    ]
  },
  {
    "volume": 880,
    "sequence": 20,
    "title": "阴阳书语",
    "author": "佚名",
    "content": [
      "乔木先枯，众子必孤。"
    ]
  },
  {
    "volume": 880,
    "sequence": 21,
    "title": "本草采萍时日歌（唐高供奉作）",
    "author": "佚名",
    "content": [
      "不在山，不在岸，采我之时七月半。",
      "选甚瘫风与缓风，些小微风都不算。",
      "豆淋酒内下三丸，铁幞头上也出汗。"
    ]
  },
  {
    "volume": 880,
    "sequence": 22,
    "title": "和剂方补骨脂丸方诗",
    "author": "佚名",
    "content": [
      "三年时节向边隅，人信方知药力殊。",
      "夺得春光来在手，青娥休笑白髭须。"
    ]
  },
  {
    "volume": 880,
    "sequence": 23,
    "title": "李廷珪藏墨诀",
    "author": "佚名",
    "content": [
      "赠尔乌玉玦，泉清研须洁。",
      "避暑悬葛囊，临风度梅月。"
    ]
  }
]