[
  {
    "volume": 877,
    "sequence": 1,
    "title": "中宗引谚",
    "author": "佚名",
    "content": [
      "冬至长于岁。"
    ]
  },
  {
    "volume": 877,
    "sequence": 2,
    "title": "贾言忠引谚",
    "author": "佚名",
    "content": [
      "军无媒，中道回。"
    ]
  },
  {
    "volume": 877,
    "sequence": 3,
    "title": "李勣引谚别张文瓘",
    "author": "佚名",
    "content": [
      "千里相送，终于一别。"
    ]
  },
  {
    "volume": 877,
    "sequence": 4,
    "title": "路励行引谚",
    "author": "佚名",
    "content": [
      "一人在朝，百人缓带。"
    ]
  },
  {
    "volume": 877,
    "sequence": 5,
    "title": "郝南容引谚",
    "author": "佚名",
    "content": [
      "三公后，出死狗。"
    ]
  },
  {
    "volume": 877,
    "sequence": 6,
    "title": "员庄谚",
    "author": "佚名",
    "content": [
      "上有天堂，下有员庄。"
    ]
  },
  {
    "volume": 877,
    "sequence": 7,
    "title": "娄师德引谚",
    "author": "佚名",
    "content": [
      "卒客无卒主人。"
    ]
  },
  {
    "volume": 877,
    "sequence": 8,
    "title": "台中谚",
    "author": "佚名",
    "content": [
      "免巡未推，只得自知。"
    ]
  },
  {
    "volume": 877,
    "sequence": 9,
    "title": "宋守敬引谚",
    "author": "佚名",
    "content": [
      "双陆无休势。"
    ]
  },
  {
    "volume": 877,
    "sequence": 10,
    "title": "雒谷谚",
    "author": "佚名",
    "content": [
      "謥洞入黄泉。"
    ]
  },
  {
    "volume": 877,
    "sequence": 11,
    "title": "三门谚",
    "author": "佚名",
    "content": [
      "古无门匠墓。"
    ]
  },
  {
    "volume": 877,
    "sequence": 12,
    "title": "张果引谚",
    "author": "佚名",
    "content": [
      "娶妇得公主，平地生公府。"
    ]
  },
  {
    "volume": 877,
    "sequence": 13,
    "title": "哥舒翰引谚",
    "author": "佚名",
    "content": [
      "狐向窟嗥不祥。"
    ]
  },
  {
    "volume": 877,
    "sequence": 14,
    "title": "代宗引谚",
    "author": "佚名",
    "content": [
      "不痴不聋，不作阿家阿翁。"
    ]
  },
  {
    "volume": 877,
    "sequence": 15,
    "title": "鬼门关谚",
    "author": "佚名",
    "content": [
      "鬼门关，十人去，九不还。"
    ]
  },
  {
    "volume": 877,
    "sequence": 16,
    "title": "河北谚",
    "author": "佚名",
    "content": [
      "山东一条葛，无事莫撩拨。"
    ]
  },
  {
    "volume": 877,
    "sequence": 17,
    "title": "李振引谚",
    "author": "佚名",
    "content": [
      "百岁奴事三岁主。"
    ]
  },
  {
    "volume": 877,
    "sequence": 18,
    "title": "王彦章引谚",
    "author": "佚名",
    "content": [
      "人死留名，豹死留皮。"
    ]
  },
  {
    "volume": 877,
    "sequence": 19,
    "title": "孙光宪《北梦琐言》引谚",
    "author": "佚名",
    "content": [
      "小舅小叔，相追相逐。"
    ]
  },
  {
    "volume": 877,
    "sequence": 20,
    "title": "谚",
    "author": "佚名",
    "content": [
      "枣子塞鼻孔，悬栖阁却种。",
      "蝉鸣蛁蟟唤，黍种糕糜断。"
    ]
  },
  {
    "volume": 877,
    "sequence": 21,
    "title": "宁茵事谚",
    "author": "佚名",
    "content": [
      "鹁鸠树上鸣，意在麻子地。"
    ]
  },
  {
    "volume": 877,
    "sequence": 22,
    "title": "鸬鹚谚",
    "author": "佚名",
    "content": [
      "鸬鹚不打脚下塘。"
    ]
  },
  {
    "volume": 877,
    "sequence": 23,
    "title": "盐铁谚",
    "author": "佚名",
    "content": [
      "扬一益二。"
    ]
  },
  {
    "volume": 877,
    "sequence": 24,
    "title": "冯翊谚",
    "author": "佚名",
    "content": [
      "苦泉羊，洛水浆。"
    ]
  },
  {
    "volume": 877,
    "sequence": 25,
    "title": "丹徒谚",
    "author": "佚名",
    "content": [
      "生东吴，死丹徒。"
    ]
  },
  {
    "volume": 877,
    "sequence": 26,
    "title": "湖州里谚",
    "author": "佚名",
    "content": [
      "放尔生，放尔命，放尔湖州做百姓。"
    ]
  },
  {
    "volume": 877,
    "sequence": 27,
    "title": "益阳谚",
    "author": "佚名",
    "content": [
      "长沙益阳，一时相卬。"
    ]
  },
  {
    "volume": 877,
    "sequence": 28,
    "title": "昭潭谚",
    "author": "佚名",
    "content": [
      "昭潭无底橘州浮。"
    ]
  },
  {
    "volume": 877,
    "sequence": 29,
    "title": "江右四郡谚",
    "author": "佚名",
    "content": [
      "筠袁赣吉，脑后插笔。"
    ]
  },
  {
    "volume": 877,
    "sequence": 30,
    "title": "徐闻谚",
    "author": "佚名",
    "content": [
      "欲拔贫，诣徐闻。"
    ]
  },
  {
    "volume": 877,
    "sequence": 31,
    "title": "陇西谚",
    "author": "佚名",
    "content": [
      "郎驱女驱，十马九驹。",
      "安阳大角，十牛九犊。"
    ]
  },
  {
    "volume": 877,
    "sequence": 32,
    "title": "荆棺峡谚",
    "author": "佚名",
    "content": [
      "九子不葬父，一女打荆棺。"
    ]
  },
  {
    "volume": 877,
    "sequence": 33,
    "title": "南中谚",
    "author": "佚名",
    "content": [
      "秋收稻，夏收头。"
    ]
  },
  {
    "volume": 877,
    "sequence": 34,
    "title": "事狐神谚",
    "author": "佚名",
    "content": [
      "无狐魅，不成村。"
    ]
  },
  {
    "volume": 877,
    "sequence": 35,
    "title": "李哲家怪引谚",
    "author": "佚名",
    "content": [
      "一鸡死，一鸡鸣。"
    ]
  },
  {
    "volume": 877,
    "sequence": 36,
    "title": "俗谚",
    "author": "佚名",
    "content": [
      "白日无谈人，谈人则害生。",
      "昏夜无说鬼，说鬼则怪至。"
    ]
  },
  {
    "volume": 877,
    "sequence": 37,
    "title": "李白名许云封谜",
    "author": "佚名",
    "content": [
      "树下彼何人，不语真吾好。",
      "语若及日中，烟霏谢成宝。"
    ]
  },
  {
    "volume": 877,
    "sequence": 38,
    "title": "许氏碑阴谜",
    "author": "佚名",
    "content": [
      "谈马砺毕，王田数七。"
    ]
  },
  {
    "volume": 877,
    "sequence": 39,
    "title": "大明寺壁语",
    "author": "佚名",
    "content": [
      "一人堂堂，二曜同光。",
      "泉深尺一，点去冰傍。",
      "二人相连，不欠一边。",
      "三梁四柱列火然，除却双勾两日全。"
    ]
  },
  {
    "volume": 877,
    "sequence": 40,
    "title": "曹著与客谜",
    "author": "佚名",
    "content": [
      "一物坐也坐，卧也坐，行也坐（客）。",
      "（著应声曰：",
      "“在官地，在私地。",
      "”）一物坐也卧，立也卧，行也卧，走也卧，卧也卧（著）（客不能对。",
      "著曰：",
      "“我谜吞得你谜。",
      "”客大惭）"
    ]
  },
  {
    "volume": 877,
    "sequence": 41,
    "title": "客题青龙寺门",
    "author": "佚名",
    "content": [
      "龛龙去东海，时日隐西斜。",
      "敬文今不在，碎石入流沙。"
    ]
  },
  {
    "volume": 877,
    "sequence": 42,
    "title": "陶穀题南唐官舍壁",
    "author": "佚名",
    "content": [
      "西川狗，百姓眼。",
      "马包儿，御厨饭。"
    ]
  },
  {
    "volume": 877,
    "sequence": 43,
    "title": "谢小娥梦盗姓名",
    "author": "佚名",
    "content": [
      "车中猴，门东草。",
      "禾中走，一日夫。"
    ]
  }
]